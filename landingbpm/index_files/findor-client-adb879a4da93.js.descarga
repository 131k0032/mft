//
// Package access is responsible for
// persisting the selected package.
//
// The package includes the room keys
// and the tickets that are selected.
//
// These variables are all stored
// within the query string.
//
angular
  .module('findor-client')
  .service('PackageAccess', function ($location, $state, Tickets) {
    console.debug( 'PackageAccess' );

    var qsParams = [
      'room1Key',
      'room2Key',
      'room3Key',
      'room4Key'
    ];

    var PackageAccess = {};

    //
    // get
    // ---
    //
    // Get retrieves the package properties
    // from the query string and de-marshalls
    // them.
    //
    PackageAccess.get = function () {
      var loc = $location.search();
      var i;
      var param;
      var pac = {};
      
      for (i = 0; i < qsParams.length; i += 1) {
        param = qsParams[i];
        pac[param] = loc[param];
      }

      var tickets = loc.tickets;
      pac.tickets = [];
      if (tickets) {
        var ids = tickets.split(',');
        var tix;
        var id;
        var quantity;
        var ticket;
        for (i = 0; i < ids.length; i += 1) {
          tix = ids[i].split(':');
          id = tix[0];
          quantity = parseInt(tix[1]);
          ticket = Tickets.get(id);
          Tickets.current = ticket;
          ticket.quantity = quantity;
          pac.tickets.push(ticket);
        }
      }

      return pac;
    };

    //
    // set
    // ---
    //
    // Sets the package properties to the
    // query string.
    //
    PackageAccess.set = function (params) {
      var i;
      var key;

      var ticketString = '';
      var ticket;
      if (params.tickets) {
        for (i = 0; i < params.tickets.length; i += 1) {
              ticket = params.tickets[i];
            if(!!ticket){
              ticketString += ticket.id + ':' + ticket.quantity + ',';
            }
        }
        params.tickets = ticketString.substring(0, ticketString.length - 1);
        if (params.tickets.length === 0) params.tickets = undefined;
      }

      for (key in params) {
        console.log('key :',key);
        console.log('params :',params[key]);
        $location.search(key, params[key]);
      }
    };

    return PackageAccess;
  });
//
// SearchAccess
// ------------
//
// Search parameters are stored in the query
// string.  This module manages access to those
// parameters.
//
// Additionally, the ID of the Orbitz solution
// that the user is currently browsing is
// stored as a state parameter.
//
angular
  .module('findor-client')
  .service('SearchAccess', function ($location, $state, CONFIG) {

    var SearchAccess = {};
    var qsParams = [
      'packageType',
      'room1',
      'room2',
      'room3',
      'room4',
      'room1Key',
      'room2Key',
      'room3Key',
      'room4Key',
      'checkIn',
      'checkOut',
      'tickets'
    ];
    var stateParams = [
      'id'
    ];

    //
    // get
    // ---
    // 
    // Retrieves the search parameters that 
    // are present in the query string.
    //
    SearchAccess.get = function () {
      console.debug( 'SearchAccess.get' );

      var search = $location.search();
      var state = $state.params;
      var result = {};
      var i;
      var param;

      for (i = 0; i < qsParams.length; i += 1) {
        param = qsParams[i];
        result[param] = search[param];
      }

      for (i = 0; i < stateParams.length; i += 1) {
        param = stateParams[i];
        result[param] = state[param];
      }

      if (!result.packageType) 
        result.packageType = 'H'; // this line should be edited to have a CONFIG fallback

      return result;
    };

    //
    // set
    // ---
    //
    // Sets the search parameters in the
    // query string.
    //
    // Accepts an object, `search`, which
    // should contain only keys defined
    // in the `qsParams` variable within 
    // this service.
    //
    SearchAccess.set = function (search) {
      var i;
      var param;

      for (i = 0; i < qsParams.length; i += 1) {
        param = qsParams[i];
        $location.search(param, search[param]);
      }
    };

    return SearchAccess;
  });angular
  .module('findor-client')
  .service('UberAccess', function (SERVER_BASE_URL, $http, $cacheFactory, $q) {
    var UberAccess = {};
    var estimateCache = $cacheFactory('UberAccess.estimate');

    //
    // estimate
    // --------
    //
    // Provides Uber cost estimates for a specified trip
    //
    // - `params`: an object containing the following keys
    //
    //    - `start_latitude`: The starting latitude of the trip
    //    - `start_longitude`: The starting longitude of the trip
    //    - `end_latitude`: The ending latitude of the trip
    //    - `end_longitude`: The ending longitude of the trip
    //
    UberAccess.estimate = function (params) {
      var deferred = $q.defer();
      var url = SERVER_BASE_URL + '/uber/estimate';
      var key = JSON.stringify(params);
      var cached = estimateCache.get(key);

      if (cached)
        deferred.resolve(cached);

      else {
        $http({
            method: 'GET',
            url: url,
            params: params
          })
          .then(function (res) {
            estimateCache.put(key, res.data.prices);
            deferred.resolve(res.data.prices);
          })
          .catch(deferred.reject);
      }

      return deferred.promise;
    };

    return UberAccess;
  });angular
