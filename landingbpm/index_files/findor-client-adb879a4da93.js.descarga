angular
  .module('findor-client')
  .factory('Price', function (CurrencyConverter, $q) {

    //
    // constructor
    // -----------
    //
    // The constructor for a `Price`
    // accepts an numerical `value`
    // and a `currency` code which
    // the value is in.
    //
    // The parameters are passed in 
    // as properties of the `params`
    // object.
    //
    var Price = function (params) {
      this.value = params.value;
      this.currency = params.currency;
    };

    //
    // convertTo
    // ---------
    //
    // This method converts the price
    // into an equivalent price in a 
    // different currency.  `currency`
    // should be the currency code to
    // convert the price to.
    //
    Price.prototype.convertTo = function (currency) {
      var deferred = $q.defer();
      
      CurrencyConverter
        .convert(this.value, this.currency, currency)
        .then(function (convertedValue) {
          var price = new Price({ value: convertedValue, currency: currency });
          deferred.resolve(price);
        })
        .catch(deferred.reject);

      return deferred.promise;
    };

    return Price;
