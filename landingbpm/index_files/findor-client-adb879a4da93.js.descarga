  .module('findor-client')
  .service('Google', function (GoogleAccess, $q) {
    var Google = {};

    //
    // getDirections
    // -------------
    //
    // Returns a Polyline that can be applied to 
    // a Leaflet Map, as well as the total distance
    // from one end of the line to the other.
    //
    // - `params`: Object containing the following keys
    //
    //    - `origin`: A string representing the origin address
    //
    //    - `destination`: A string representing the destination address
    //
    Google.getDirections = function (params) {
      var deferred = $q.defer();

      GoogleAccess
        .directions(params)
        .then(function (res) {
          res.polylineLayer = L.Polyline.fromEncoded(res.polyline);
          deferred.resolve(res);
        })
        .catch(deferred.reject);

      return deferred.promise;
    };

    //
    // getDurations
    // -------------
    //
    // Returns an object containing the travel duration in human-readable text
    // For both modes, `walking` and `transit`
    //
    // - `params`: Object containing the following keys
    //    - `origin`: A string representing the origin address
    //    - `destination`: A string representing the destination address
    //
    Google.getDurations = function (params) {
      var deferred = $q.defer();

      GoogleAccess
        .durations(params)
        .then(function (res) {
          deferred.resolve(res);
        })
        .catch(deferred.reject);

      return deferred.promise;
    };

    return Google;
