    function checkoutError(message, data) {
      console.debug('BookCtrl.checkoutError', message, data);
      if (!message) {
        $scope.errors = { server: true, show: true };
      } else {
        $scope.errors = { message: message, show: true };
      }
    }

    /**
     * Process Booking
     *
     *
     * @todo Should use the $scope.package.tickets instead of selectedTicket.
     *
     * @param info
     * @returns {*}
     */
    $scope.makeBooking = function makeBooking(info) {
      console.debug('BookCtrl.makeBooking', info);
      console.debug('BookCtrl.makeBooking : $scope.package', $scope.package);

      if ($scope.bookingLocked) {
        return;
      }

      if (!checkValidity())
        return setErrors();

      // Semi-ghetto validation, just trying to test some error output.
      if (!info.cc || !info.cc.number) {
        return checkoutError('Missing credit card number.');
      }

      $scope.bookingLocked = true;
      cfpLoadingBar.start();

      var _transaction = {
        stripeToken: {
          hotel: new StripeKey().hotel,
          admission: new StripeKey().festival,
        },
        hotelTransaction: null,
        admissionTransaction: null,
        stripeData: !info.cc ? {} : {
          name: info.cc.name,
          number: info.cc.number,
          cvc: info.cc.cvc,
          exp_month: info.cc.exp_month,
          exp_year: info.cc.exp_year,
          address_line1: info.cc.address_1,
          address_line2: info.cc.address_2,
          address_city: info.cc.city,
          address_state: info.cc.state,
          address_country: info.cc.country,
          address_zip: info.cc.zip
        },
        ticketDetail: null,
      };

      // Set Ticket Detail
      if ($scope.selectedTicket) {
        _transaction.ticketDetail = {
          quantity: $scope.selectedTicket.quantity,
          amount: (($scope.selectedTicket.price.value + $scope.selectedTicket.taxesAndFees.value) * 100), // convert into cents
          currency: $scope.selectedTicket.price.currency,
          description: $scope.selectedTicket.name,

        };
      }

      //+$scope.selectedTicket.taxesAndFees.value
      //taxesAndFees:$scope.selectedTicket.taxesAndFees.value
      // Set primary key.
      stripe.setPublishableKey(_transaction.stripeToken.hotel);

      console.debug('ResultsSingleCtrl.makeBooking', _transaction);

      return stripe.card.createToken(_transaction.stripeData)
        .then(function (response) {
          console.log('token for findor created', response);

          _transaction.hotelTransaction = response.id;

          // set other key
          stripe.setPublishableKey(_transaction.stripeToken.admission);

          // create other token
          return stripe.card.createToken(_transaction.stripeData);

        })
        .then(function (response) {
          console.log('token for admission sale created', response);

          _transaction.admissionTransaction = response.id;

          var _post_body = {
            'stripeToken': _transaction.hotelTransaction,
            'customer-name': info.guest.fullname || (info.guest.first_name + ' ' + info.guest.last_name),
            'customer-email': info.guest.email,
            'customer-phone': info.guest.phone,
            'key': $scope.selectedRoom.key
          };

          // Send `stripeFestivalToken` only when ticket is selected
          if (_transaction.ticketDetail) {

            angular.extend(_post_body, {
              'stripeFestivalToken': _transaction.admissionTransaction,
              'ticket[quantity]': _transaction.ticketDetail.quantity,
              'ticket[description]': _transaction.ticketDetail.description,
              'ticket[amount]': _transaction.ticketDetail.amount,
              'ticket[currency]': _transaction.ticketDetail.currency
            });

          }



          return $http.post(SERVER_BASE_URL + '/v2/booking/', _post_body);

        })
        .then(function (response) {
          console.log('Booking V2 response', response);

          if (!response || !response.data || !response.data.booking_code) {
            return checkoutError("Payment failed.", response);
