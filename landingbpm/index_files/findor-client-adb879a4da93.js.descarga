  .directive('photosSlider', function ($interval) {
    return {
      restrict: 'E',
      replace: true,
      templateUrl: '/static/templates/photos-slider.directive.tpl.html',
      link: function (scope, elem, attrs) {
        var children = elem.children();
        var photosDiv = children.eq(0);
        var gradientsDiv = children.eq(1);
        var leftGradient = gradientsDiv.children().eq(0);
        var rightGradient = gradientsDiv.children().eq(1);
        var arrowsDiv = children.eq(2);
        var leftArrow = arrowsDiv.children().eq(0);
        var rightArrow = arrowsDiv.children().eq(1);
        var interval = null;

        scope.showLeftArrow = false;
        scope.showRightArrow = false;

        scope.startMovingLeft = function() {
          $interval.cancel(interval);
          interval = $interval(function() {
            moveLeft();
          }, 1);
        };

        scope.startMovingRight = function() {
          $interval.cancel(interval);
          interval = $interval(function() {
            moveRight();
          }, 1);
        };

        scope.stopMoving = function() {
          $interval.cancel(interval);
        };

        var refresh = function() {
          var clientWidth = photosDiv[0].clientWidth;
          var scrollWidth = photosDiv[0].scrollWidth;
          var scrollLeft = photosDiv[0].scrollLeft;
          var buffer = 5;
          if (scrollLeft >= buffer) {
            scope.showLeftArrow = true;
          }
          else {
            scope.showLeftArrow = false;
          }
          if (scrollLeft + clientWidth <= scrollWidth - buffer) {
            scope.showRightArrow = true;
          }
          else {
            scope.showRightArrow = false;
          }
        };

        var moveLeft = function() {
          photosDiv[0].scrollLeft -= 1;
        };

        var moveRight = function() {
          photosDiv[0].scrollLeft += 1;
        };

        photosDiv.bind("scroll", function() {
          scope.$apply(function() {
            refresh();
          });
        });

        scope.$watch(function () {
          return photosDiv[0].scrollWidth;
        }, function (newValue, oldValue) {
          refresh();
        });

      },
      scope: {
        photos: '=?'
      }
    };
  });angular
