    function formatDate(date, format) {
      var days = date.getDate() < 10 ? "0" + date.getDate() : date.getDate();
      var month = date.getMonth() < 9 ? "0" + (date.getMonth() + 1) : (date.getMonth() + 1);
      var year = date.getFullYear();
      switch (format) {
        case "yyyy-mm-dd":
          return year + "-" + month + "-" + days;
        case "yy-mm-dd":
          return year + "-" + month + "-" + days;
        case "dd-mm-yy":
          return year + "-" + month + "-" + days;
        case "dd-mm-yyyy":
          return year + "-" + month + "-" + days;
        default:
          return year + "-" + month + "-" + days;
      }


