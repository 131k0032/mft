    function getDateRangeString() {
      var startDate = moment($scope.CONFIG.startDate, 'YYYY-MM-DD') || moment();
      var endDate = moment($scope.CONFIG.endDate, 'YYYY-MM-DD') || moment();
      var lng = $scope.language.code;


      switch (lng) {
        case "cat":
          //case catalan "del D MMMM YYYY"
          var string = startDate.format('D') + ' - ' + endDate.format('D MMMM YYYY');
          break;
        case "en":
          //case inglesh "on MMMM D YYYY"   
          var string = startDate.format('MMMM D') + ' - ' + endDate.format('D, YYYY');
          break;
        case "es":
          //case spanish "el D de MMMM YYYY"
          var string = startDate.format('D') + ' - ' + endDate.format('D MMMM YYYY');
          break;
        case "fr":
          //case frensh "le D MMMM YYYY"
          var string = startDate.format('D') + ' - ' + endDate.format('D MMMM YYYY');
          break;
        default:
          var string = startDate.format('MMMM D') + ' - ' + endDate.format('D, YYYY');
          break;
      }





      return string;
    }

    function getDayEvent() {

      var endDate = moment($scope.CONFIG.endDate, 'YYYY-MM-DD');
      var lng = $scope.language.code;
      var eventDay = endDate.format("MMMM D YYYY");

      switch (lng) {
        case "cat":
          //case catalan "del D MMMM YYYY"
          eventDay = "del " + endDate.format("D MMMM YYYY");
          break;
        case "en":
          //case inglesh "on MMMM D YYYY"   
          eventDay = "on " + endDate.format("MMMM D, YYYY");
          break;
        case "es":
          //case spanish "el D de MMMM YYYY"
          eventDay = "el " + endDate.format("D") + " de " + endDate.format("MMMM YYYY");
          break;
        case "fr":
          //case frensh "le D MMMM YYYY"
          eventDay = "le " + endDate.format("D MMMM YYYY");
          break;
        default:
          eventDay = "on " + endDate.format("MMMM D YYYY");
          break;
      }


      return eventDay;
