  .module('findor-client')
  .service('BookingInfo', function (Search, BookingInfoAccess, $rootScope) {

    var BookingInfo = {
      info: {
        guests: [],
      },
      validations: {
        name: /^[A-Za-z]+$/,
        fullname: /^[A-Za-z ]+$/,
        cc: {
          number: /^[0-9]+$/,
          month: /^[0-9]{1,2}$/,
          year: /^[0-9]{4}$/,
          cvc: /^[0-9]{3,}$/
        }
      },
      shipping: {
        sameAddress: false,
        address: {}
      }
    };

    //
    // save
    // ----
    //
    // This method saves booking info into 
    // local storage so that it can be
    // reloaded later.
    //
    var save = function () {
      BookingInfoAccess.set(BookingInfo.info);
    };

    //
    // load 
    // ----
    //
    // This method loads booking info from the
    // data-access layer.
    //
    var load = function () {
      var info = BookingInfoAccess.get();
      if (info) BookingInfo.info = info;
    };

    // 
    // setGuests
    // ---------
    //
    // This method ensures that BookingInfo always
    // has information for one guest per room
    // being booked.
    //
    var setGuests = function () {
      var search = Search.get();
      var guests = BookingInfo.info.guests;
      while (guests.length < search.numRooms) {
        guests.push({});
      }
      while (guests.length > search.numRooms) {
        guests.pop();
      }
    };

    //
    // getShippingAddress
    // ------------------
    //
    // This method builds the shipping address
    // string.
    //
    BookingInfo.getShippingAddress = function () {
      var bi = BookingInfo;
      var address = {
        name: bi.cc.name
      };

      if (bi.shipping.sameAddress) {
        address.street1 = bi.cc.address_1;
        address.street2 = bi.cc.address_2;
        address.country = bi.cc.country;
        address.state = bi.cc.state;
        address.city = bi.cc.city;
        address.postal = bi.cc.postal;
      }

      else {
        address = bi.shipping.address;
      }

      var addressStr = address.street1 + '\n'
        + (address.street2 || '') + '\n'
        + address.city + ', ' + address.state + ', ' + address.country 
        + '\n' + address.postal;

      return addressStr;
    };

    Search.addListener('changed', setGuests);
    load();
    setGuests();

    $rootScope.$watch(function () {
      return BookingInfo;
    }, function (newValue, oldValue) {
      save();
    }, true);

    return BookingInfo;
