angular
  .module('findor-client')
  .service('Search', function (SearchAccess) {

    var Search = new EventEmitter();

    // Search types include:
    var packageTypes = {
      'H': 'hotelOnly',  
      'HT': 'hotelPlusTicket',
      'HA': 'hotelPlusAir',
      'HAT': 'hotelPlusAirPlusTicket'
    };

    // the current Search search
    var search = {
      packageType: 'H',
      setHotelOnly: function () {
        Search.set({packageType: 'H'});
      },
      setHotelPlusTicket: function () {
        Search.set({packageType: 'HT'});
      },
      setHotelPlusAir: function () {
        Search.set({packageType: 'HA'});
      },
      setHotelPlusAirPlusTicket: function () {
        Search.set({packageType: 'HAT'});
      },
      setTicket: function (yes) {
        var pkg = this.packageType;
        var lastLetter = pkg.substr(pkg.length - 1);
        if (yes && lastLetter !== 'T')
          pkg += 'T';
        else if (!yes && lastLetter === 'T')
          pkg = pkg.substr(0, pkg.length - 1);

        if (pkg !== this.packageType)
          Search.set({ packageType: pkg });
      },
      getKeys: function () {
        var i;
        var keys = [];
        var key;

        for (i = 1; i <= 4; i += 1) {
          key = this['room' + i + 'Key'];
          if (key) keys.push(key);
        }
        
        return keys;
      }
    };

    //
    // refresh
    // -------
    //
    // this updates the calculated values of
    // `numGuests` and `numNights`.
    //
    var refresh = function () {
      var i;

      // set the number of guests
      search.numGuests = (search.room1 || 0)
                       + (search.room2 || 0) 
                       + (search.room3 || 0) 
                       + (search.room4 || 0);

      // set the number of nights of the stay
      if (!search.checkIn || !search.checkOut)
        search.numNights = 0;

      var range = moment().range(search.checkIn, search.checkOut);
      var days = 0;
      range.by('days', function () {
        days += 1;
      });
      search.numNights = days - 1;

      // set the types for the search
      search.types = Search.getTypes();

      // set the number of rooms for the search
      search.numRooms = 0;
      for (i = 1; i <= 4; i += 1) {
        if (search['room' + i])
          search.numRooms += 1;
      }

      // build the guests array
      search.guests = [
        search.room1 || 0, 
        search.room2 || 0, 
        search.room3 || 0,
        search.room4 || 0
      ];
    };

    //
    // get
    // ---
    // 
    // gets the type of Search being
    // searched for
    //
    Search.get = function () {
      return search;
    };

    //
    // load
    // ----
    // 
    // loads the Search from the query
    // string
    //
    Search.load = function () {
      console.debug( 'Search.load' );
      
      var rawSearch = SearchAccess.get();
      var i, roomKey;
      search.packageType = rawSearch.packageType;
      search.checkIn = rawSearch.checkIn ? moment(rawSearch.checkIn) : undefined;
      search.checkOut = rawSearch.checkOut ? moment(rawSearch.checkOut) : undefined;
      search.room1 = parseInt(rawSearch.room1) || undefined;
      search.room2 = parseInt(rawSearch.room2) || undefined;
      search.room3 = parseInt(rawSearch.room3) || undefined;
      search.room4 = parseInt(rawSearch.room4) || undefined;
      search.numGuests = (search.room1 || 0)
                       + (search.room2 || 0) 
                       + (search.room3 || 0) 
                       + (search.room4 || 0);
      for (i = 1; i <= 4; i += 1) {
        roomKey = 'room' + i + 'Key';
        search[roomKey] = rawSearch[roomKey];
      }
      search.tickets = rawSearch.tickets;

      refresh();
      Search.emit('loaded');
    };

    //
    // set
    // ---
    // 
    // updates the search
    //
    Search.set = function (update) {

      // update the local search object
      var key;
      for (key in update) {
        if (key === 'checkIn' || key === 'checkOut')
          search[key] = moment(update[key]);
        else
          search[key] = update[key];
      }
      refresh();

      // deep copy the search object
      // and persist it 
      var rawUpdate = {};
      for (key in search) {
        rawUpdate[key] = search[key];
      }
      rawUpdate.types = undefined;
      rawUpdate.checkIn = rawUpdate.checkIn.format('YYYY-MM-DD');
      rawUpdate.checkOut = rawUpdate.checkOut.format('YYYY-MM-DD');
      SearchAccess.set(rawUpdate);

      // dispatch a changed event 
      Search.emit('changed');
    };



    //
    // getTypes
    // --------
    // 
    // Gets a list an object for boolean
    // types of the object
    //
    Search.getTypes = function () {
      var key;
      var types = {};
      for (key in packageTypes) {
        var type = packageTypes[key];
        types[type] = search.packageType === key;
      }
      return types;
    };

    return Search;
