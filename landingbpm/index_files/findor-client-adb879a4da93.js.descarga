//
// This module manages the language
// which the user is using to browse
// Findor with.  It communicates with
// the data-access layer to set the
// language and dispatches events when
// the language changes.
//
angular
  .module('findor-client')
  .service('Language', function (LanguageAccess, gettextCatalog) {
    console.debug( 'Language' );

    var Language = new EventEmitter();
    var languages = {
      'en': {
        code: 'en',
        gettext: 'en',
        moment: 'en',
        display: 'English',
        orbitz: 'en_US'
      },
      'fr': {
        code: 'fr',
        gettext: 'fr_FR',
        moment: 'fr_FR',
        display: 'FranÃ§ais',
        orbitz: 'fr_FR'
      },
      'es': {
        code: 'es',
        gettext: 'es',
        moment: 'es',
        display: 'EspaÃ±ol',
        orbitz: 'es_ES'
      },
      'cat':{
        code:'cat',
        gettext:'cat',
        moment:'cat',
        display:'CatalÃ¡n',
        orbitz:'ca_ES',
        optional:true
      }
    };
    var language = null;

    //
    // load
    // ----
    //
    // Loads the language service
    // from the data-access layer.
    //
    Language.load = function () {
      console.debug( 'Language.load' );

      var current;
      language = LanguageAccess.get();
      current = languages[language];
      gettextCatalog.currentLanguage = current.gettext;
      moment.lang(current.moment);
      Language.emit('loaded');
    };

    //
    // set
    // ---
    //
    // 1. Sets the current language.
    //
    // 2. Updates the data-access layer.
    //
    // 3. Modifies the application language
    //    & the language for moment.
    //
    // 4. Dispatches the changed event.
    //
    Language.set = function (code) {
      console.debug( 'Language.set', code );

      var current = languages[code];
      language = code;
      LanguageAccess.set(code);
      gettextCatalog.currentLanguage = current.gettext;
      moment.lang(current.moment);
      Language.emit('changed');
    };

    //
    // get
    // ---
    //
    // Gets the current language
    //
    Language.get = function () {
      console.debug( 'Language.get' );
      return languages[language];
    };

    //
    // getAll
    // ------
    //
    // Returns the full list of languages.
    //
    Language.getAll = function () {
      console.debug( 'Language.getAll' );
      return languages;
    };

    return Language;
  });
