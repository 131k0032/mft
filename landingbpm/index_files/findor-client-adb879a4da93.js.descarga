  .service('Uber', function (UberAccess, $q, Price, Currency) {
    var Uber = {};

    //
    // uberXEstimate
    // -------------
    //
    // Provides a regular (no surge) UberX estimate for the 
    // provided trip.
    //
    // - `params`: an object containing the following keys
    //
    //    - `start_latitude`: The starting latitude of the trip
    //    - `start_longitude`: The starting longitude of the trip
    //    - `end_latitude`: The ending latitude of the trip
    //    - `end_longitude`: The ending longitude of the trip
    //
    Uber.getUberXEstimate = function (params) {
      var deferred = $q.defer();

      UberAccess
        .estimate(params)
        .then(function (estimates) {
          var currency = Currency.get();
          var promises = [];
          var lowEstimate;
          var highEstimate;
          var estimateCurrency;
          var surgeMultiplier;
          var uberXEstimate;
          var uberXEstimates = estimates.filter(function (estimate) {
            return estimate.display_name === 'uberX';
          });

          if (uberXEstimates.length === 0) 
            return deferred.reject('no uberX estimate');

          uberXEstimate = uberXEstimates[0];
          estimateCurrency = uberXEstimate.currency_code;
          surgeMultiplier = uberXEstimate.surge_multiplier;
          lowEstimate = new Price({ 
            value: uberXEstimate.low_estimate / surgeMultiplier, 
            currency: estimateCurrency 
          });
          highEstimate = new Price({
            value: uberXEstimate.high_estimate / surgeMultiplier,
            currency: estimateCurrency
          });

          promises.push(lowEstimate.convertTo(currency.display));
          promises.push(highEstimate.convertTo(currency.display));

          $q.all(promises).spread(function (low, high) {
            var estimate = currency.symbol + Math.floor(low.value) + "-" + currency.symbol + Math.ceil(high.value);
            deferred.resolve(estimate);
          });
        })
        .catch(deferred.reject);

      return deferred.promise;
    };

    return Uber;
  });angular
