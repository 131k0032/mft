  .controller('InvoiceCtrl', function ($scope, $state, BookingInfo, Invoice, CreditCards) {
    $scope.bi = BookingInfo;
    $scope.creditCards = CreditCards.get();
    $scope.creditCardLogos = CreditCards.getLogos();
    $scope.creditCardClasses = CreditCards.getClasses();

    var invoiceCode = $state.params.code;
    Invoice
      .get(invoiceCode)
      .then(function (invoice) {
        $scope.invoice = invoice;
      })
      .catch(function (err) {
        if (err.status === 404) {
          $scope.notFound = true;
        } else if (err.status === 418) {
          $scope.paid = true;
          $scope.confirmationNumber = invoiceCode;
        } else {
          throw "Unhandled GET /invoice error" + err;
        }
      });

    $scope.payInvoice = function (form) {
      $scope.errors = {};
      form.$setDirty();
      if(!form.$valid) {
        $scope.errors.missingInfo = true;
        return;
      }

      Invoice
        .pay(invoiceCode)
        .then(function (res) {
          delete $scope.invoice;
          $scope.paid = true;
          $scope.confirmationNumber = invoiceCode;
        })
        .catch(function (err) {
          if (err.status === 402) {
            $scope.errors.payment = true;
          } else {
            throw "Unhandled POST /invoice/:id/pay error" + err;
          }
        });
    };
  });angular
