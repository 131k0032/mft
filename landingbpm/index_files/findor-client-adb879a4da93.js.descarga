        /**
         * Convert new Room format to legacy room format.
         *
         * - Convert rates from cents into floats
         *
         * @param room
         * @returns {*}
         */
        function applyLegacyRoomFormat( room ) {

            // Map our new room types to follow Orbitz-esque format to make template changes more straight forward.
            var _room_type_map = {
                "Single bed": "1BED",
                "Queen size bed": "1BED_QUEEN",
                "King size bed": "1BED_KING",
                "2 Double beds": "2BED_DOUBLE",
                "2 Twin beds": "2BED_TWIN",
                "Suite": "SUITE",
                "Special attendee rate": "SPECIAL"
            };

            // Map our new room types to Orbitz type. (This should hopefully not be needed anymore)
            var _orbitz_room_group = {
                "2 Double beds": "2BED",
                "2 Twin beds": "2BED",
                "King size bed": "1BED",
                "Queen size bed": "1BED",
                "Single bed": "1BED",
                "Suite": "SUITE"
            };

            if( room.legacyFormat ) {
                return room;
            }

            // taxes_and_fees -> subTotal
            if( room.rate && !room.subTotal ) {
                room.subTotal = { value: ( room.rate / 100 ), currency: room.currency };
            }

            // blank it out until we know what to do with it.
            if( !room.localChargesTotal ) {
                room.localChargesTotal = { value: 0, currency: room.currency };
            }


            // taxes_and_fees -> taxesAndFees
            if( (room.taxes_and_fees >= 0) && !room.taxesAndFees ) {
                room.taxesAndFees = { value: ( room.taxes_and_fees / 100 ), currency: room.currency };
            }

            // total_price -> total
            if( room.total_price && !room.total ) {
                room.total = { value: ( room.total_price / 100 ), currency: room.currency };
            }


            // Set printable title. If for some reason category is missing, nothing we can do. - potanin@UD
            room.display_name = room.category || 'Unknown';

            // Detect "Room Type"
            if( room.category && _room_type_map[ room.category ] ) {
                room.categoryType = room.category;
                room.category = [ _room_type_map[ room.category  ] ];
                room.orbitzGroup = _orbitz_room_group[ room.categoryType ];
            }

            // we have no new value for these fields.
            room.strike = { value: 0 };
            room.taxTotal = { value: 0 };
            room.feeTotal = { value: 0 };


            // Mark flag that the legacy format has already been applied to this object
            room.legacyFormat = true;

            return room;
        }

        return Hotel;
