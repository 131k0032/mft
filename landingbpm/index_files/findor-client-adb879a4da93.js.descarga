              return sum + roomRate.roomRate[0].supplierFees.checkoutFees.total;
            }
          }
        }, 0);
        return sum;
      };

      data.getRoomRate = function (r) {
        return _.where(data.rooms.roomRates, {room: r})[0];
      };

      data.getPhotos = function () {
        return _.where(data.details.content.media, {type: 'property'});
      };

      data.getDescription = function () {
        return _.where(data.details.content.description, {type: 'property'});
