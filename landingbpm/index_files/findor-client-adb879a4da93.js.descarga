      data.stars = [ blank(1), blank(2), blank(3), blank(4), blank(5) ];
      data.stayDetails = data.stayDetails ? stayDetails(data.stayDetails) : undefined;

      data.hotels.featuredCount = 0;
      _.forEach(data.hotels.hotel, function (_hotel) {
        _hotel = hotel(_hotel);
        var star = data.stars[_hotel.details.starRating - 1];
        if (star) {
          var totalCost = _hotel.totalCost;
          star.count += 1;
          if (star.minInclusive === null || totalCost < star.minInclusive) {
            star.minInclusive = Math.ceil(totalCost);
          }
          if (star.maxInclusive === null || totalCost > star.maxInclusive) {
            star.maxInclusive = Math.ceil(totalCost);
