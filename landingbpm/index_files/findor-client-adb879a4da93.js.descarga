  .module('findor-client')
  .service('Booker', function ($q, BookingInfo, Repricer, Price, Search, HotelAccess, Language, StripeKey, StripeToken, featureFlags) {
    console.debug( 'Booker' );

    var Booker = {};
    var deferred = $q.defer();

    //
    // buildRooms
    // ----------
    //
    // Builds the room node for the request.
    //
    var buildRooms = function (rooms, guests) {
      var i;
      var room;
      var requestRooms = {};
      var total = 0;
      var currency;
      var keys = [];
      for (i = 1; i < 4; i += 1) {
        room = rooms[i];
        if (room) {
          requestRooms['room' + i] = { guest: guests[i - 1] };
          total += room.selected.total.value;
          currency = room.selected.total.currency;
        }
      }
      requestRooms.total = new Price({ value: total, currency: currency });
      requestRooms.key = Search.get().getKeys().join('-');
      return requestRooms;
    };

    //
    // book
    // ----
    // 
    // Performs a booking request
    //
    Booker.book = function () {
      console.debug( 'Booker.book' );

      var cc = BookingInfo.cc;
      var guests = BookingInfo.info.guests;
      var pac = Repricer.get();
      var rooms = buildRooms(pac.orbitz.rooms, guests);
      var ticket; 

      if (pac.tickets[0]) {
        ticket = {
          currency: pac.tickets.total.currency,
          amount: Math.ceil(pac.tickets.total.value * 100),
          description: pac.tickets[0].name,
          quantity: pac.tickets[0].quantity
        };

        ticket.metadata = {
          'Email Address': guests[0].email
        };


        // v1
        if (pac.tickets[0].shipTickets) {
          ticket.metadata['Shipping Address'] = BookingInfo.getShippingAddress();
        }

      }

      // If not using internalHotels, use the legacy Orbibtz API call that doens't need Stripe tokens.
      if( !featureFlags.isOn('internalHotels') ) {

        return HotelAccess.book( {
          payment: {
            cc: cc
          },
          customer: guests[ 0 ],
          ticket: ticket,
          hotel: {
            type: 'orbitz',
            rooms: rooms
          }
        } );

      }

      // query parameters
      var parameters = {
        locale: Language.get().orbitz
      };

      var customer = guests[0];

      //request stripe card tokens
      generateStripeTokens(cc, ticket).then(function(response){

        var hotelResponse = 0;
        var ticketResponse = 1;

        //query data
        var data = {
          'stripeToken': response[hotelResponse].id,
          'stripeKey': new StripeKey().hotel,
          'stripeFestivalToken': response[ticketResponse] ? response[ticketResponse].id : '',
          'stripeFestivalKey': new StripeKey().festival,
          'customer-name': buildName(customer),
          'customer-email': customer.email ,
          'customer-phone': customer.phone,
          key: Search.get().getKeys().pop(),
          'ticket[quantity]': getTicketData('quantity'),
          'ticket[description]': getTicketData('description'),
          'ticket[amount]': getTicketData('amount'),
          'ticket[currency]': getTicketData('currency')
        };

        var params = {
          parameters: parameters,
          data: data
        };

        deferred.resolve(HotelAccess.book(params));
      });

      return deferred.promise;

    };

    function getTicketData(ticket, field) {
      if (angular.isDefined(ticket)) {
        return ticket[field] || '';
      } else {
        return '';
      }
    }

    function generateStripeTokens(card, ticket) {

      var promises = [];
      var stripeKey = new StripeKey();

      var hotelTokenPromise = StripeToken.create(stripeKey.hotel, card);
      promises.push(hotelTokenPromise);

      //Tickets?
      if (angular.isDefined(ticket)) {
        var eventTokenPromise = StripeToken.create(stripeKey.festival, card);
        promises.push(eventTokenPromise);
      }

      return $q.all(promises);

    }

    function buildName(customer) {
      return customer.first_name + customer.last_name;
    }

    return Booker;
  });
