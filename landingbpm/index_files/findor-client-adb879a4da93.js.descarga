      data.getCheckInTime = function () {
        return data.details.content.checkInTime;
      };

      data.getCheckOutTime = function () {
        return data.details.content.checkOutTime;
      };

      data.distanceToVenues = _.map(CONFIG.venues, function (venue) {
        var hotelLatLng = {lat: data.details.address.latitude, lng: data.details.address.longitude};
        return angular.extend(venue, {distance: Geometry.distance(hotelLatLng, venue.location)});
      });

      data.summaryDistance = _.reduce(data.distanceToVenues, function (sum, venue) {
        return sum + venue.distance;
      }, 0) / data.distanceToVenues.length;

      if (data.rooms.roomRates) {
        data.hasFreeCancellation = data.rooms.roomRates[0].roomRate[0].cancelPolicy ? data.rooms.roomRates[0].roomRate[0].cancelPolicy.freeCancellable : false;
        data.totalCostInclusive = data.rooms.totalCostOfRooms.totalCostInclusive.value;
        if (data.details.content) {
          data.displayPhoto = _.where(data.details.content.media, {type: 'property'})[0];
        }
        data.totalCost = _.reduce(data.rooms.roomRates, function (sum, roomRates) {
          var roomRate = roomRates.roomRate[0];
          return roomRate.totalCost ? sum + roomRate.totalCost.value : sum;
        }, 0);
        data.totalStrikeRate = _.reduce(data.rooms.roomRates, function (sum, roomRates) {
          var roomRate = roomRates.roomRate[0];
          return roomRate.totalStrikeRate ? sum + roomRate.totalStrikeRate.value : sum;
        }, 0);
      }

      return data;
