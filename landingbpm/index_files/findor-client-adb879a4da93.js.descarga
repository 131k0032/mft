                    rate = hotel.rooms[roomNumber][roomKey];
                    rate.quantity = 1;

                    // get room-specific amentities
                    rate.roomAmenities = _.where( this.detailedAmenities, {'category': 'room' });

                    if (!rate.freeCancellable)
                        freeCancellable = false;

                    else
                        hasFreeCancellableRoom = true;

                    rooms[roomNumber][roomKey] = new Room(rate);

                    if (keys) {
                        var roomKeyAndQuantity = (keys[roomNumber -1] || '').split(':');
                        if (roomKey === roomKeyAndQuantity[0]) {
                            rate.quantity = parseInt(roomKeyAndQuantity[1]);
                            rooms[roomNumber].selected = rate;
                        }
                    }
                }

                // if there is no selected room, we need to set the lowest priced
                // room as selected.  since Object.keys isn't supported in
                // older browsers, we'll iterate the keys in the object and
                // break after the first.
                
                //it didnt work ever cause the rate is an object and was treated as number!!! 
                
                if( (!keys || keys.length === 0) && !rooms[roomNumber].selected) {
                    console.log();
                    var minRate = -1, roomKey;
                    _.each(rooms[roomNumber], function( room, key) {
                        rate=(typeof room.rate=='object')?room.rate.value:room.rate;
                        if( minRate === -1 || ( minRate !== -1 && rate < minRate )) {
                            minRate = rate;
                            roomKey = key;
                        }
                    });
                    if( roomKey ) {
                        rooms[roomNumber].selected = rooms[roomNumber][roomKey];
                    }
                }

                if (rooms[roomNumber].selected) {
                    updateTotals(rooms[roomNumber].selected);
                }
            }


            this.rooms = rooms;
            this.total = new Price({ value: total, currency: currency });
            this.subTotal = new Price({ value: subTotal, currency: currency });
            this.strike = new Price({ value: strike, currency: currency });
            this.taxTotal = new Price({ value: taxTotal, currency: currency });
            this.feeTotal = new Price({ value: feeTotal, currency: currency });
            this.localChargesTotal = new Price({ value: localChargesTotal, currency: currency });
            this.taxesAndFees = new Price({ value: taxesAndFeesTotal, currency: currency });
            this.taxes = taxes;
            this.fees = fees;
            this.localCharges = localCharges;
            this.freeCancellable = freeCancellable;
            this.hasFreeCancellableRoom = hasFreeCancellableRoom;
            this.tags = hotel.tags;

            this.distances = [];

            var totalDistance = 0;
            for (i = 0; i < CONFIG.venues.length; i += 1) {
                var venue = CONFIG.venues[i];

                var latLng = { lat: this.address.latitude, lng: this.address.longitude };
                var distance = Geometry.distance(latLng, venue.location);
                this.distances.push(distance);
                totalDistance += distance;
            }
            this.avgDistance = totalDistance / this.distances.length;

            // console.dir( this );

        };

        //
        // updateStats
        // -----------
        //
        // Updates all the details for the
        // hotel, such as prices, etc.
        //
        Hotel.prototype.updateStats = function () {
            console.debug( 'Hotel.updateStats' );

            var rooms = this.rooms;
            var key;
            var room;
            var priceTotal = 0;
            var currency;
            var inclusiveTotal = 0;
            var strikeTotal = 0;
            var taxTotal = 0;
            var feeTotal = 0;
            var taxesAndFees = 0;
            var taxes = [];
            var fees = [];
            var localCharges = [];

            for (key in rooms) {
                room = rooms[key].selected;
                if (room) {
                    currency = room.currency;
                    priceTotal += room.subTotal.value * room.quantity;
                    inclusiveTotal += room.total.value * room.quantity;
                    strikeTotal += room.strike.value;
                    taxTotal += room.taxTotal.value * room.quantity;
                    feeTotal += room.feeTotal.value * room.quantity;

                    var roomTaxesAndFees = ('object' === typeof room.taxesAndFees ? room.taxesAndFees.value : room.taxesAndFees);

                    taxesAndFees += (roomTaxesAndFees * room.quantity);

                    // taxes = taxes.concat(room.taxes);
                    // fees = fees.concat(room.fees);
                    taxes = taxes.concat({
                        value: roomTaxesAndFees * room.quantity,
                        currency: currency,
                        description: 'Hotel Taxes and Fees',
                    });
                    localCharges = room.localCharges ? localCharges.concat(room.localCharges) : localCharges;
                }
            }

            this.total = new Price({ value: inclusiveTotal, currency: currency });
            this.subTotal = new Price({ value: priceTotal, currency: currency });
            this.strike = new Price({ value: strikeTotal, currency: currency });
            this.taxTotal = new Price({ value: taxTotal, currency: currency });
            this.feeTotal = new Price({ value: feeTotal, currency: currency });
            this.taxes = taxes;
            this.fees = fees;
            this.localCharges = localCharges;
            this.taxesAndFees = new Price({ value: taxesAndFees, currency: currency });
        };

        //
        // setRoom
        // -------
        //
        // Sets a room for the hotel
        //
        // - `roomIndex` The index of the room to set
        // - `key` The key of the room to set
        // - `quantity` The quantity of the room to set
        //
        Hotel.prototype.setRoom = function (roomIndex, key, quantity) {
            console.debug( 'Hotel.setRoom', roomIndex, key );

            // roomIndex may be numeric, but in our case
            var rooms = this.rooms[ roomIndex ];

            var firstKey = Object.keys(rooms)[0];

            rooms.selected = rooms[key]
                ? rooms[key]
                : rooms[firstKey];

            rooms.selected.quantity = quantity;

            this.updateStats();
        };

        /**
         * Identify Orbitz fields and create "detailedAmenities" as supported natively by V2 API
         * hasBeach
         * hasGym
         * hasDining
         * hasBar
         * hasInternet
         *
         * @param hotel
         * @returns {Array}
         */
        function applyAmenityFormat( hotel ) {
            console.debug( 'Hotel.applyAmenityFormat', hotel );

            // Map Orbitz amentity titles to icons.
            var amenityMap = {
                "parking": "parking",
                "room service": "room-service",
                "no smoking rooms/facilities": "no-smoking",
                "business center": "business-center",
                "safe": "safe",
                "tv": "tv",
                "desk with lamp": "desk",
                "desk": "desk",
                "wifi": "wifi",
                "pet friendly": "pet-friendly",
                "shower": "shower"
            };

            var roomRelatedAmentities = [ 'safe', 'tv', 'desk with lamp', 'wifi', 'pet friendly', 'shower' ];

            var detailedAmenities = [];

            if( hotel.hasBeach ) {
                detailedAmenities.push({ name: 'Beach', category: 'hotel', icon: 'beach-access'});
            }

            if( hotel.hasPool ) {
                detailedAmenities.push({ name: 'Pool', category: 'hotel', icon: 'pool'});
            }

            if( hotel.hasGym ) {
                detailedAmenities.push({name: 'Gym', category: 'hotel', icon: 'gym'});
            }

            if( hotel.hasDining ) {
                detailedAmenities.push({name: 'Dining', category: 'hotel', icon: 'dining'});
            }

            if( hotel.hasBar ) {
                detailedAmenities.push({name: 'Bar', category: 'hotel', icon: 'bar'});
            }

            if( hotel.hasInternet ) {
                detailedAmenities.push({name: 'Internet', category: 'hotel', icon: 'wifi'});
            }

            if( hotel.amenities ) {
                angular.forEach( hotel.amenities, function eachAmenity( amenity ) {

                    var _slug = amenity.toLowerCase();
                    var _category = 'hotel';

                    if( !amenityMap[ _slug ] ) {
                        return;
                    }

                    // amenity is related to rooms
                    if( roomRelatedAmentities.indexOf( _slug ) >= 0 ) {
                        _category = 'room';
                    }

                    detailedAmenities.push({
                        name: amenity,
                        category: _category,
                        icon: amenityMap[amenity.toLowerCase()]
                    });

                });
            }

            // Prevent having too many amenities.
            if( detailedAmenities.length > 10 ) {
                // detailedAmenities.slice( 0, 11 );
            }

            return detailedAmenities;

        }

        /**
         * Apply "New" format to the Orbitz objects, that supports the ux-v2 styles.
         *
         * @author potanin@UD
         * @param hotel
         * @returns {*}
         */
        function applyNewFormat( hotel ) {
            console.debug( 'Hotel.applyNewFormat', hotel );

            // If new format is applied, return object.
            if( hotel.newFormat ) {
                return hotel;
            }

            // use the hotel name as the description_header, a field used by ux-v2
            hotel.description_header = hotel.name;

            // use first hotel description as the description_body, a field used by ux-v2
            if( hotel.descriptions && hotel.descriptions.length && hotel.descriptions[0] ) {
                hotel.description_body = '<p>' + hotel.descriptions[0] + '</p>';
            } else {
                hotel.description_body = '';
            }

            var _photos = [];

            angular.forEach( hotel.photos, function eachPhoto( photo ) {

                // add https to domains that we know support it
                _photos.push( photo.replace( 'http://www.tnetnoc', 'https://www.tnetnoc' ) );

            });


            hotel.photos = _photos;

            // Set flag that this hotel object has been converted to support new format
            hotel.newFormat = true;

            return hotel;

        }


        /**
         * Convert response objects to legacy "Orbitz" format.
         *
         * This should be within hotel/room factories but they are not invoked consistently throughout the logic so this is best place to guarantee.
         *
         * @potanin @UD
         * @param hotel
         * @returns {*}
         */
        function applyLegacyFormat( hotel ) {
            console.debug( 'Hotel.applyLegacyFormat', hotel );

            // If legacy format already applied, do nothing. (Happens because data is cached sometimes)
            if( hotel.legacyFormat ) {
                return hotel;
            }

            // preserve legacy room array for iteration
            var _rooms = _.clone( hotel.rooms );

            // create new blank array, we treat all new rooms as the legacy room type of "1".
            hotel.rooms = {};

            // Build room array. Orbitz returns multiple results based on search
            angular.forEach( _rooms, function( roomData ) {
                var _normalizedRoom = applyLegacyRoomFormat( roomData );
                hotel.rooms[ 1 ] = hotel.rooms[ 1 ] || {};
                hotel.rooms[ 1 ][ roomData.key ] = _normalizedRoom;
            });


            // Backwards compat for address. - potanin@UD
            hotel.address = {
                latitude: hotel.location ? hotel.location.latitude_longitude[0] : null,
                longitude: hotel.location ? hotel.location.latitude_longitude[1] : null,
                street1: hotel.location ? hotel.location.address : null,
                stateProvince: { value: hotel.location ? hotel.location.state : null },
                city: hotel.location ? hotel.location.city : null,
                country: { value: hotel.location ? hotel.location.country : null },
                postal_code: hotel.location ? hotel.location.postal_code : null
            };

            // store detailed amentiies, lowercase for better compat with css font
            hotel.detailedAmenities = _.map( hotel.amenities, function normalizeAmenities( data ) {

                return {
                    name: data.name,
                    category: ( data.category || '' ).toLowerCase(),
                    icon: ( data.icon || '' ).toLowerCase(),
                    featured: data.featured,
                };

            });
            //  adding additional amenities not from the list
            addDynamicAmenities(hotel);
            // Store new photo format in detailPhotos
            hotel.detailPhotos = hotel.photos;

            // Backwards compat for photos
            hotel.photos = [];

            var mainPhoto = _.find( hotel.detailPhotos, { 'category': 'MAIN' });
            if (mainPhoto) {
                hotel.photos.push(mainPhoto.main_photo);
            }

            _.each( hotel.detailPhotos, function( item ) {
                if (item.category !== 'MAIN' && item['featured_thumbnail'] === true) {
                    hotel.photos.push( item.main_photo );
                }
            });

            // If `MAIN` photo is not found and `featured` photos are also not found,
            // Select at least one photo
            if( hotel.photos.length === 0 && hotel.detailPhotos.length > 0 ) {
                hotel.photos.push( hotel.detailPhotos[0].main_photo);
            }

            hotel.checkInTime = hotel.checkin_time;
            hotel.checkOutTime = hotel.checkout_time;


            // old format had an array of descriptions.
            hotel.descriptions = [ hotel.description ];

            // Set legacy features
            hotel.hasBeach = false;
            hotel.hasPool = false;
            hotel.hasGym = false;
            hotel.hasDining = false;
            hotel.hasBar = false;
            hotel.hasInternet = false;

            // This is VERY temporary, we will need to handle amneties differently very shortly. - potanin@UD
            angular.forEach( hotel.amenities, function mapAmenty( amentiy ) {

                if( amentiy.name === 'Fitness Centre' ) {
                    hotel.hasGym = true;
                }

                if( amentiy.name === 'Indoor Pool' ) {
                    hotel.hasPool = true;
                }

            });

            // Convert amneties to simple array for list
            hotel.amenities = _.map(hotel.amenities, function( amentiy ) {
                return amentiy.name;
            });


            // Set flag that this hotel object has been converted to support legacy format
            hotel.legacyFormat = true;
            // console.log('hotel amen', hotel.detailedAmenities);
            return hotel;

        }

