angular
  .module('findor-client')
  .factory('Room', function (Price) {

    //
    // constructor
    // -----------
    //
    // Builds a `Room`
    //
    var Room = function (room) {
      console.debug( 'Room', room.key );

      // New API returns prices in cents..
      var pricesInCents = [ 'total_price', 'retail_rate', 'rate', 'taxes_and_fees' ];

      // all price fields
      var prices = [ 'strike', 'subTotal', 'taxTotal', 'taxesAndFees', 'total', 'feeTotal', 'total_price', 'retail_rate', 'rate', 'taxes_and_fees' ];

      var key;

      // apply special price formatting to select keys
      for (key in room) {

        if( prices.indexOf( key ) > 0 ) {
          // console.debug( 'Room -> appply price formatting to', key );

          var _value = 'object' === typeof room[key] && room[key] ? room[key].value : room[key];
          var _currency = 'object' === typeof room[key] && room[key] ? room[key].currency : room.currency;

          // convert prices in cents to standard
          if( pricesInCents.indexOf( key ) >= 0 ) {
            _value = ( _value  / 100 );
          }

          this[key] = new Price({
            value: _value,
            currency: _currency
          });

        } else {
          this[key] = room[key];
        }

      }

      var taxes = [];
      var tax;
      if (room.taxes) {
        for (var i = 0; i < room.taxes.length; i += 1) {
          tax = new Price({ value: room.taxes[i].value, currency: room.taxes[i].currency });
          for (key in room.taxes[i]) {
            if (key !== 'value' && key !== 'currency')
              tax[key] = room.taxes[i][key];
          }
          taxes.push(tax);
        }
      }
      this.taxes = taxes;

      // Add room features (v2 and v1)
      this.roomAmenities = this.roomAmenities || [];

      if( this.freeCancellable ) {
        this.roomAmenities.push({ 'name': 'Can Cancel', 'category': 'room', 'icon': 'can-cancel' });
      }

      // Lame way to give legacy objects a title by combining categories
      if( !this.title && 'object' === typeof this.category ) {
          // console.log('this.category' , this.category);
        this.title = this.category.join( ' or ' );
      }

      console.debug( 'Room -> result', this );
    };


    return Room;
  });

