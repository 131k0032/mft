    function generateBookingDateTime() {
      var checkIn = $state.params.checkIn;
      var checkOut = $state.params.checkOut;
      var mCheckIn = moment(checkIn, 'YYYY-MM-DD');
      var mCheckOut = moment(checkOut, 'YYYY-MM-DD');
      if (mCheckIn && mCheckOut && $scope.package.orbitz) {
        $scope.checkInDateTime = '' + mCheckIn.format('ddd, MMM D') + ' ' + $scope.package.orbitz.checkInTime.slice(0, -3);
        $scope.checkOutDateTime = '' + mCheckOut.format('ddd, MMM D') + ' ' + $scope.package.orbitz.checkOutTime.slice(0, -3);
      }
    }

    function getSelectedRoomAndTicket() {
      if (!$scope.package) {
        return;
      }
      if ($scope.package.tickets.length > 0) {
        $scope.selectedTicket = $scope.package.tickets[0];
        console.log($scope.selectedTicket);
      }
      _.each($scope.package.orbitz.rooms, function (roomOptions, key) {

        $scope.selectedRoom = roomOptions.selected;

        if ($scope.selectedRoom) {

          // $scope.selectedRoom.checkin_instructions=$sce.trustAsHtml($scope.selectedRoom.checkin_instructions);

          console.log("selected Room : ", $scope.selectedRoom);
          console.log("tipo de precio ", typeof ($scope.selectedRoom.total_price));

          if ($scope.selectedRoom.custom_fees) {

            $scope.selectedRoom.salesTax = $scope.selectedRoom.quantity * ($scope.selectedRoom.custom_fees.sales_tax || 0) / 100;
            $scope.selectedRoom.cityTax = $scope.selectedRoom.quantity * ($scope.selectedRoom.custom_fees.hotel_city_tax || 0) / 100;

          }




          $scope.selectedRoom.findorFees = $scope.selectedRoom.quantity * $scope.selectedRoom.findor_fee / 100;


          var txf = typeof ($scope.selectedRoom.taxes_and_fees) == 'object' ? $scope.selectedRoom.taxes_and_fees.value : ($scope.selectedRoom.taxes_and_fees / 100);
          $scope.selectedRoom.taxServiceFees = ($scope.selectedRoom.quantity * txf);

          var fslx = typeof ($scope.selectedRoom.total_price) == 'object' ? $scope.selectedRoom.total_price.value : ($scope.selectedRoom.total_price / 100);
          $scope.selectedRoom.TotalBrut = fslx * $scope.selectedRoom.quantity;


          $scope.selectedRoom.totalNet = (fslx * $scope.selectedRoom.quantity) - $scope.selectedRoom.taxServiceFees - $scope.selectedRoom.findorFees;
          $scope.selectedRoom.totalNet = parseFloat($scope.selectedRoom.totalNet.toFixed(2));

          console.log("currency Hotel(no meaning):", $scope.packageCurrency);
          console.log("currency Room             :", $scope.selectedRoom.currency);
          if ($scope.selectedTicket) {
            console.log("currency ticket           :", $scope.selectedTicket.price.currency);
          }
          console.log("============================================");
          console.log("Total net            : ", $scope.selectedRoom.totalNet);
          console.log("Sales Tax            : ", $scope.selectedRoom.salesTax);
          console.log("Tax&Services Fees    : ", $scope.selectedRoom.taxServiceFees);
          console.log("Findor Travel svc fee: ", $scope.selectedRoom.findorFees);
          console.log("City Tax             : ", $scope.selectedRoom.cityTax);
          console.log("Total Hotel          : ", $scope.selectedRoom.TotalBrut);
          if ($scope.selectedTicket) {
            console.log("Ticket Price(uni)         :", $scope.selectedTicket.price.value);
            console.log("Ticket Q                  :", $scope.selectedTicket.quantity);
            console.log("Ticket Taxes&Fees(uni)    :", $scope.selectedTicket.taxesAndFees.value);
            console.log("Total Ticket              :", ($scope.selectedTicket.taxesAndFees.value + $scope.selectedTicket.price.value) * $scope.selectedTicket.quantity);
            console.log("==============================================");
            console.log("Total :", ($scope.selectedTicket.taxesAndFees.value + $scope.selectedTicket.price.value) * $scope.selectedTicket.quantity + $scope.selectedRoom.TotalBrut)
          }
        }


      });
    }
