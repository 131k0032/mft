  .service('resultsMapHelper', function (CONFIG, Geometry, hotelHelper, $state, $window, $timeout, $filter, Currency, CurrencyConverter) {

    var
        map, // the leaflet map object
        $map, // jquery object of the map div
        tileLayer,  // layer of tiles for the map
        latLngs = [], // all latLngs in the hotel marker layer and the venue marker layer
        hotelMarkerLayer, // cluster marker layer for hotel results
        venueMarkerLayer, // marker layer for venues
        focusLayer, // the layer that loaded during single view
        lastCenter, // the last center latLng before loading single view
        lastZoom, // the last zoom level before loading single view
        $rotationStyle, // style DOM element that rotates map pins
        resizeListener; // the resize listener function

    return {
      initMap: initMap,
      focusMap: focusMap,
      unfocusMap: unfocusMap,
      populateHotelClusterGroup: populateHotelClusterGroup,
      getMapRotation: getMapRotation,
      refreshMap: refreshMap,
      removeResizeListener: removeResizeListener
    };


    /**
     * Generates the CSS style required to rotate the map appropriately when
     * focusing on a single result
     *
     * @param {object} coords The coordinates to focus on (contains `lat` and `lng`)
     * @return {object} The CSS ng-style object for the #map div
     */
    function getMapRotation (coords) {
      var venue = CONFIG.venues[0].location
        , rotation = Geometry.getRotationToHorizontal(coords, venue);
      return {
        transform: 'rotate(' + rotation + 'deg)'
      };
    }


    /**
     * Focuses the map on a single hotel
     *
     * @param {object} result The result from the Orbitz shopping request
     */
    function focusMap (lat, lng) {
      var dest = { lat: lat, lng: lng };

      map.removeLayer(hotelMarkerLayer);
      map.removeLayer(venueMarkerLayer);
      lastCenter = map.getCenter();
      lastZoom = map.getZoom();

      disableMapControl();
      refreshMap();
      addResizeListener(dest);
      addPinRotationStyle(dest);
      $timeout(function() {
        showPathToVenue(dest);
      }, 1);
    }

    /**
     * Displays the path to the venue
     */
    function showPathToVenue (resultTmp) {
      if (focusLayer) {
        map.removeLayer(focusLayer);
      }
      var location = CONFIG.venues[0].location;
      var eventLatLng = L.latLng(location.lat, location.lng);
      var resultLatLng = L.latLng(resultTmp.lat, resultTmp.lng);
      var avgLat = (resultTmp.lat + location.lat) / 2;
      var avgLng = (resultTmp.lng + location.lng) / 2;
      var distanceFromCenter = Geometry.distance(resultTmp, location) / 60; // TODO: figure out what this magic number means
      var leftPt = {lat: avgLat, lng: avgLng - distanceFromCenter};
      var rightPt = {lat: avgLat, lng: avgLng + distanceFromCenter};

      focusLayer = L.layerGroup();
      focusLayer.addTo(map);

      map.fitBounds([new L.latLng(leftPt.lat, leftPt.lng), new L.latLng(rightPt.lat, rightPt.lng)], {reset: true});

      var polyLine = L.polyline([resultLatLng, eventLatLng], {color: 'white'});
      var logo = CONFIG.venues[0].logo;
      var venueIcon = L.divIcon({
        html: '<div style="width:' + logo.iconSize[0]
                + 'px; height:' + logo.iconSize[1]
                + 'px; background-image:url(' + logo.iconUrl + ')'
                + ';"></div>',
        className: 'map-venue'
      });
      var hotelIcon = L.divIcon({
        html: '<span class="sprite pin"></span>',
        className: 'map-pin'
      });
      var venuePin = L.marker(eventLatLng, {icon: venueIcon});
      var hotelPin = L.marker(resultLatLng, {icon: hotelIcon});

      polyLine.addTo(focusLayer);
      venuePin.addTo(focusLayer);
      hotelPin.addTo(focusLayer);

      focusLayer.addTo(map);

      return focusLayer;
    }

    /**
     * Returns the map to regular view
     */
    function unfocusMap () {
      if (focusLayer) {
        map.removeLayer(focusLayer);
      }
      hotelMarkerLayer.addTo(map);
      venueMarkerLayer.addTo(map);
      enableMapControl();
      removeResizeListener();
      removePinRotationStyle();
      refreshMap();
      map.setView(lastCenter, lastZoom, {reset: true});
    }

    /**
     * Disables all map controls
     */
     function disableMapControl () {
      map.dragging.disable();
      map.touchZoom.disable();
      map.doubleClickZoom.disable();
      map.scrollWheelZoom.disable();
      map.boxZoom.disable();
      map.keyboard.disable();
      $('.leaflet-control-zoom').hide();
    }

    /**
     * Enables all map controls
     */
    function enableMapControl () {
      map.dragging.enable();
      map.touchZoom.enable();
      map.doubleClickZoom.enable();
      map.scrollWheelZoom.enable();
      map.boxZoom.enable();
      map.keyboard.enable();
      $('.leaflet-control-zoom').show();
    }

    /**
     * Initializes  the map object
     *
     * @param {string} domID The id of the DOM object to initialize the map in
     */
    function initMap (domID) {
      map = L.map(domID, {
        center: [CONFIG.map.defaultCenter.lat, CONFIG.map.defaultCenter.lng],
        zoom: CONFIG.map.defaultZoom,
        minZoom: CONFIG.map.minZoom,
        attributionControl: false,
        zoomControl: false,
        scrollWheelZoom: false,
      });

      addZoomControl();
      addTileLayer();
      addHotelClusterGroup();
      addVenueLayer();

      $map = $('#' + domID);

      refreshMap();
      lastCenter = map.getCenter();
      lastZoom = map.getZoom();
    }


    /**
     * Fits all the added objects into the b
     */
    function fitBounds () {
      map.fitBounds(latLngs);
    }


    /**
     * Adds all the venues to the map
     */
    function addVenueLayer () {
      venueMarkerLayer = L.layerGroup();
      venueMarkerLayer.addTo(map);
      _.forEach(CONFIG.venues, function (venue) {
        var icon = L.icon(venue.logo),
            latLng = L.latLng(venue.location.lat, venue.location.lng),
            marker = L.marker(latLng, {icon: icon});
        venueMarkerLayer.addLayer(marker);
        latLngs.push(latLng);
