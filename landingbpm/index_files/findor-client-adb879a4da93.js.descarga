// CurrencyConverter
// -----------------
//
// The `CurrencyConverter` is responsible
// for accessing currency conversion rates
// through the `CurrencyConversionAccess`
// and for converting amounts using those
// conversion rates.
//
angular
  .module('findor-client')
  .service('CurrencyConverter', function ($q, CurrencyConversionAccess) {

    var CurrencyConverter = {};

    //
    // convert
    // -------
    //
    // This method uses `CurrencyConversionAccess`
    // to retrieve the current conversion rates.
    // Then, it converts `fromAmount` from `fromCurrency`
    // into `toCurrency`.
    //
    CurrencyConverter.convert = function (fromAmount, fromCurrency, toCurrency) {
      var deferred = $q.defer();

      CurrencyConversionAccess
        .get(fromCurrency)
        .then(function (rates) {
          var rate = rates[toCurrency];
          var toAmount;

          if (!rate)
            throw "Could not convert to currency " + toCurrency;
          
          toAmount = rate * fromAmount;
          deferred.resolve(toAmount);
        })
        .catch(deferred.reject);

      return deferred.promise;
    };

    return CurrencyConverter;
  });//
