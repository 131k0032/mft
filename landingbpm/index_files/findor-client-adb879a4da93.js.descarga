    }

    /**
     * Creates the hotel cluster group
     */
    function addHotelClusterGroup () {
      hotelMarkerLayer = new L.MarkerClusterGroup({
        showCoverageOnHover: false,
        spiderfyOnMaxZoom: false,
        disableClusteringAtZoom: map.getMaxZoom(),
        iconCreateFunction: iconCreate
      });
      map.addLayer(hotelMarkerLayer);
    }

    /**
     * Adds the tile layer to the map
     */
    function addTileLayer () {
      L.TileLayer.APP = L.TileLayer.extend({_update: updateExtended});
      var tileLayerOptions = {
        subdomains: "abcd",
        key: CONFIG.map.id,
        minZoom: CONFIG.map.minZoom,
        maxZoom: CONFIG.map.maxZoom,
        detectRetina: true
      };
      tileLayer = new L.TileLayer.APP("https://{s}.tiles.mapbox.com/v3/{key}/{z}/{x}/{y}.png", tileLayerOptions);
      tileLayer.addTo(map);
    }

    /**
     * Adds zoom control to the map
     */
    function addZoomControl () {
      map.addControl(L.control.zoom({position: CONFIG.map.controlPlacement}));
    }

    /**
     * A function for determining the tile window to load
     *
     * This is an extension of a default Leaflet function
     */
    function updateExtended () {
      if (!this._map) { return; }
      var bounds = this._map.getPixelBounds(),
          zoom = this._map.getZoom(),
          tileSize = this.options.tileSize;
      if (zoom > this.options.maxZoom || zoom < this.options.minZoom) {
        return;
      }
      var width = bounds.max.x - bounds.min.x,
          height = bounds.max.y - bounds.min.y;
      var largestDimension = Math.max(width, height);
      var largestRotatedDimension = 2 * Math.sqrt(largestDimension * largestDimension / 2);
      var centerPoint = bounds.getCenter();
      var nwTilePoint = new L.Point(
            Math.floor((centerPoint.x - largestRotatedDimension / 2) / tileSize),
            Math.floor((centerPoint.y - largestRotatedDimension / 2) / tileSize)),
          seTilePoint = new L.Point(
            Math.floor((centerPoint.x + largestRotatedDimension / 2) / tileSize),
            Math.floor((centerPoint.y + largestRotatedDimension / 2) / tileSize)),
          tileBounds = new L.Bounds(nwTilePoint, seTilePoint);
      this._addTilesFromCenterOut(tileBounds);
      if (this.options.unloadInvisibleTiles || this.options.reuseTiles) {
        this._removeOtherTiles(tileBounds);
      }
    }

    /**
    * Generates an icon for the map
    */
    function iconCreate (cluster) {
      var childCount = cluster.getChildCount();
      var multiples = 5;
      var c = ' amount-' + (Math.floor(childCount / multiples) * multiples).toString();
      var icon = L.divIcon({
        html: '<div><span>' + childCount + '</span></div>',
        className: 'marker-cluster' + c,
        iconSize: new L.Point(50, 50)
      });
      return icon;
    }

