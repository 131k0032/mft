  .module('findor-client')
  .service('GoogleAccess', function ($http, $q, SERVER_BASE_URL, $cacheFactory) {
    var GoogleAccess = {};
    var directionsCache = $cacheFactory('GoogleAccess.directions');
    var durationsCache = $cacheFactory('GoogleAccess.durations');
    var GOOGLE_BASE_URL = SERVER_BASE_URL + '/google';
    var durationsThreshold = 45 * 60; // under 45 mins

    //
    // directions
    // ----------
    //
    // Queries the Google Directions API to get directions
    // from the `origin` to the `destination`.
    //
    // - `params`: Object containing the following keys
    //    - `origin`: String representing the origin address
    //    - `destination`: String representing the destination
    //          address
    //
    GoogleAccess.directions = function (params) {
      var deferred = $q.defer();
      var url = GOOGLE_BASE_URL + '/directions';
      var key = JSON.stringify(params);
      var cached = directionsCache.get(key);

      if (cached)
        deferred.resolve(cached);

      else {
        $http({
            method: 'GET',
            url: url,
            params: params
          })
          .then(function (res) {
            directionsCache.put(key, res.data);
            deferred.resolve(res.data);
          })
          .catch(deferred.reject);
      }

      return deferred.promise;
    };

    //
    // durations
    // ----------
    //
    // Queries the Google Distance Matrix API to get travel durations
    // from the `origin` to the `destination`
    // For both modes, `walking` and `transit`
    //
    // - `params`: Object containing the following keys
    //    - `origin`: String representing the origin address
    //    - `destination`: String representing the destination address
    //
    GoogleAccess.durations = function (params) {
      var deferred = $q.defer();

      var key = JSON.stringify(params);
      var cached = durationsCache.get(key);

      if (cached) {
        deferred.resolve(cached);
      } else {
        var walkingParams = angular.extend( {}, params, { mode: 'walking' } );
        var transitParams = angular.extend( {}, params, { mode: 'transit' } );

        $q.all([
            getDuration(walkingParams),
            getDuration(transitParams)
          ])
          .spread(function (walkingDuration, transitDuration) {
            var durations = { };
            if (walkingDuration && walkingDuration.value <= durationsThreshold) {
              durations['walking'] = walkingDuration.text;
            }
            if (transitDuration) {
              durations['transit'] = transitDuration.text;
            }
            
            durationsCache.put(key, durations);
            deferred.resolve(durations);
          });
      }

      return deferred.promise;
    };

    //
    // Get the length of time it takes to travel a route specified by
    // `origin` and `destination`
    //
    // - `response`: Object containing the following keys
    //   - `value`: expressed in seconds
    //   - `text`:  textual representation
    //
    function getDuration (params) {
      var deferred = $q.defer();
      var url = GOOGLE_BASE_URL + '/durations';
      var key = JSON.stringify(params);
      var cached = durationsCache.get(key);

      if (cached)
        deferred.resolve(cached);

      else {
        $http({
            method: 'GET',
            url: url,
            params: params
          })
          .then(function (res) {
            durationsCache.put(key, res.data);
            deferred.resolve(res.data);
          })
          .catch(function(err) {
            deferred.resolve(null);
          });
      }

      return deferred.promise;
    }

