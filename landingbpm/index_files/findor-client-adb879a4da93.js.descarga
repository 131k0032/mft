  .directive('directionsMap', function () {
    return {
      restrict: 'E',
      replace: false,
      templateUrl: '/static/templates/directions-map.directive.tpl.html',
      link: function (scope, elem, attrs) {

        // generate tile layer
        var tileOptions = {
          subdomains: "abcd",
          key: scope.config.id,
          minZoom: scope.config.minZoom,
          maxZoom: scope.config.maxZoom,
          detectRetina: true
        };
        var tiles = L.tileLayer("https://{s}.tiles.mapbox.com/v3/{key}/{z}/{x}/{y}.png", tileOptions);

        // generate venue layer
        var venue = scope.venue;
        var venues = L.layerGroup();
        var icon = L.icon(venue.logo);
        var latLng = L.latLng(venue.location.lat, venue.location.lng);
        var marker = L.marker(latLng, { icon: icon });
        venues.addLayer(marker);

        // build map
        var mapOptions = {
          zoomControl: false,
          attributionControl: false
        };
        scope.map = L.map('directions-map-container', mapOptions)
          .addLayer(tiles)
          .addLayer(venues)
          .setView(scope.venue.location, 15);

        // disable map controls
        scope.map.dragging.disable();
        scope.map.touchZoom.disable();
        scope.map.doubleClickZoom.disable();
        scope.map.scrollWheelZoom.disable();  
        scope.map.boxZoom.disable();
        scope.map.keyboard.disable();
      },
      controller: function ($scope, $window, Google, Uber, Evenement) {
        var fitBounds = function () {
          $scope.map.fitBounds($scope.bounds);
        };

        $scope.eventName = Evenement.get().name;

        $scope.$watch('hotel', function (hotel) {
          if (hotel) {

            // generate hotel layer
            var hotelIcon = L.divIcon({
              html: '<span class="sprite pin"></span>',
              className: 'map-pin',
              iconAnchor: [20, 59]
            });
            var hotelLocation = { lat: hotel.address.latitude, lng: hotel.address.longitude };
            var hotelPin = L.marker(hotelLocation, {icon: hotelIcon});
            $scope.map.addLayer(hotelPin);

            // get directions to hotel
            var origin = hotel.address.latitude + ',' + hotel.address.longitude;
            var destination = $scope.venue.location.lat + ',' + $scope.venue.location.lng;

            Google
              .getDirections({
                origin: origin,
                destination: destination
              })
              .then(function (directions) {

                // add the directions to map
                var style = $scope.config.directions || {};
                if (!style.color) style.color = 'white';
                directions.polylineLayer.setStyle(style);
                $scope.map.addLayer(directions.polylineLayer);

                // set the bounds for the map
                var latDifference = Math.abs(directions.bounds.northeast.lat - directions.bounds.southwest.lat);
                var lngDifference = Math.abs(directions.bounds.northeast.lng - directions.bounds.southwest.lng);
                var latDelta = 0.4 * latDifference;
                var lngDelta = 0.4 * lngDifference;
                var northeast = [directions.bounds.northeast.lat + latDelta, directions.bounds.northeast.lng + lngDelta];
                var southwest = [directions.bounds.southwest.lat - latDelta, directions.bounds.southwest.lng - lngDelta];
                $scope.bounds = [southwest, northeast];
                $scope.distance = directions.distance / 1000;

                // On initial render, the map is shown cut, need to update
                $scope.map.invalidateSize();

                fitBounds();
              });

            Google
              .getDurations({
                origin: origin,
                destination: destination
              })
              .then(function (durations) {
                $scope.walkingDuration = durations.walking?durations.walking.slice(0,-1):'';
                $scope.transitDuration = durations.transit?durations.transit.slice(0,-1):'';
              });

            Uber.getUberXEstimate({
              start_latitude: hotel.address.latitude,
              start_longitude: hotel.address.longitude,
              end_latitude: $scope.venue.location.lat,
              end_longitude: $scope.venue.location.lng
            }).then(function (uberEstimate) {
              $scope.uberEstimate = uberEstimate;
            });
          }
        });

        $window.addEventListener('resize', fitBounds);
      },
      scope: {
        hotel: '=?',
        venue: '=?',
        config: '=?'
      }
    };
  });angular
