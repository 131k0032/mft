  .module('findor-client')
  .service('bookingHelper', function (Tickets) {
    return {
      needProvince: needProvince,
      provinceOrState: provinceOrState,
      getTotal: getTotal,
      prepayPolicy: prepayPolicy,
      getChargeNode: getChargeNode
    };

    /** 
     * Retrieves the prepay policy for a room
     * @param {object} room The room (Orbitz roomRates object)
     */
    function prepayPolicy (room) {
      return _.where(room.roomRate[0].description, {type: 'PRE_PAY_POLICY'})[0].value;
    }

    /**
     * Determines the total due at booking
     */
    function getTotal (rules) {
      return rules.hotel.totalCostInclusive;
    }

    /**
     * Determines whether the user is required to enter their province
     */
    function needProvince (cardCountry) {
      var code;
      if (!cardCountry || !cardCountry.selected) { return false; }
      code = cardCountry.selected.code;
      return code === 'CA' || code === 'US' || code === 'AU';
    }

    /**
     * Determines whether the country has provinces or states
     */
    function provinceOrState (cardCountry) {
      if (cardCountry && cardCountry.selected && cardCountry.selected.code === 'CA') {
        return 'Province';
      }
      return 'State';
    }

    /**
     * Returns the appropriate charge node to be sent to the findor_server, 
     * to be shipped to orbitz.
     */
    function getChargeNode (searchStyle, creditCard, totalPeople, emailAddress, shippingAddress) {
      var charge, people;
      var costPerPerson, costWithShipping;
      var description;
      var shippingAddressStr = "";
      var shipping = null;

      if (searchStyle === 'hotel')
        return undefined;

      people = totalPeople === 1 ? "person" : "people";

      costPerPerson = Tickets.current.price.value + Tickets.current.taxesAndFees.value;
      costWithShipping = (costPerPerson * totalPeople) + (Tickets.current.shippingFee.value || 0);
      description = Tickets.current.description + " for " + totalPeople + " " + people;

      if (shippingAddress) {
        shippingAddressStr = shippingAddress.street1;
        shippingAddressStr += shippingAddress.street2 ? ", " + shippingAddress.street2 : "";
        shippingAddressStr += "\n" + shippingAddress.city + " " + shippingAddress.stateProvince.value;
        shippingAddressStr += "\n" + (shippingAddress.country.selected ? shippingAddress.country.selected.code : shippingAddress.country);
        shipping = {
          'Shipping Address': shippingAddressStr
        };
      }

      charge = {
        amount: Math.ceil(costWithShipping * 100),
        currency: Tickets.current.price.currency,
        description: description,
        receipt_email: emailAddress,
        card: {
          number: creditCard.number,
          exp_month: creditCard.expMonth.length === 1 ? "0" + creditCard.expMonth : creditCard.expMonth,
          exp_year: creditCard.expYear,
          name: creditCard.billingName,
          cvc: creditCard.verificationNumber,
          address_line1: creditCard.address.street1,
          address_line2: creditCard.address.street2,
          address_city: creditCard.address.city,
          address_zip: creditCard.address.postalCode,
          address_state: creditCard.address.stateProvince.value,
          address_country: creditCard.address.country.value
        },
        metadata: shipping
      };

      return charge;
    }

  });
