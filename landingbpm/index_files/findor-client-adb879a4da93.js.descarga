// Package
// -------
//
// A package is a solution for the user.
// It can include tickets, hotels, etc.
//
angular
  .module('findor-client')
  .factory('Package', function (Currency, Price, Search, PackageAccess, $q) {
    console.debug( 'Package' );

    var Package = function () {
      this.orbitz = {
        total: 0,
        subTotal: 0,
        strike: 0
      };
      this.tickets = [];
    };

    //
    // updatePackage
    // -------------
    //
    // Updates the values for the package.
    // These values include:
    //
    // - `subTotal`
    // - `taxes`
    // - `fees`
    // - `total`
    // - `strike`
    //
    // Similarly, subkey `tickets`
    // will contain all of the values described
    // above.  For example, `orbitz.taxes` will
    // contain the total amount
    //
    Package.prototype.updatePackage = function () {
      console.debug( 'Package.updatePackage' );

      var deferred = $q.defer();
      this.updateTickets()
        .then(function () {
            // console.log(this);
            // console.log('this package', this);
            // this part is for show hotels with promo on the top of the page
            this.isOnTop = false;
            if(
                ((!!this.orbitz.tags && this.orbitz.tags.length) || this.orbitz.freeCancellable) ||
                    !!this.label ||
                (!!this.tickets[0] && !!this.tickets[0].discount && !!this.tickets[0].discount.amount && this.tickets[0].discount.amount > 0)
            ){
                this.isOnTop = true;
            }
            if(this.orbitz.isSoldOut){
                this.isOnTop = false;
            }
          this.total = this.orbitz.total.value + this.tickets.total.value;
          this.subTotal = this.orbitz.subTotal.value + this.tickets.subTotal.value;
          this.strike = this.orbitz.strike.value + this.tickets.strike.value;
          this.taxesAndFees = this.orbitz.taxesAndFees.value + this.tickets.taxesAndFees.value;

          if (this.tickets.taxes.value > 0)
