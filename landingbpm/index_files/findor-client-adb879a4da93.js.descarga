  .module('findor-client')
  .service('Shopping', function (Search, HotelAccess, $q, Geometry, Currency, Language, CONFIG, Hotel, Package, Tickets, StripeTicket, DirtyCustomPackages) {
    console.debug('Shopping');

    var Shopping = new EventEmitter();
    var solutions = [];

    //
    // shopHotels
    // ----------
    //
    // Performs a hotel only shopping
    // request.  Returns a standard
    // solution.
    //
    var shopHotels = function (options) {
      console.debug( 'Shopping.shopHotels', options );

      var solutions = [];
      var deferred = $q.defer();

      var query = {
        checkIn: options.checkIn.format('YYYYMMDD'),
        checkOut: options.checkOut.format('YYYYMMDD'),
        city: CONFIG.search.city,
        // currency: Currency.get().orbitz,
        language: Language.get().orbitz,
        room1: options.room1,
        room2: options.room2,
        room3: options.room3,
        room4: options.room4
      };

      HotelAccess
        .shop(query)
        .then(function (hotels) {

          if( !hotels || !hotels.length ) {
            console.debug( 'Shopping.shopHotels', 'no hotels returned' );
            return deferred.reject(new Error( 'No hotels found.' ));
          }

          // Iterate over returned hotels.
          angular.forEach( hotels, function eachHotel( singleHotel ) {

            var hotel = new Hotel(singleHotel);
            var package = new Package();
            package.setOrbitzItem(hotel);
            solutions.push(package);

          });

          deferred.resolve(solutions);
        })
        .catch(function (err) {
          deferred.reject(err);
        });

      return deferred.promise;
    };

    //
    // shopHotelsAndTickets
    // --------------------
    //
    // Performs a search fo
    var shopHotelsAndTickets = function (options) {
      var deferred = $q.defer();
      var search = Search.get();
      var hotels = shopHotels(options);
      hotels
        .then(function (solutions) {
          var i;
          for (i = 0; i < solutions.length; i += 1) {
            solutions[i].addTicket(Tickets.list[0], search.numGuests);
          }
          deferred.resolve(solutions);
        })
        .catch(deferred.reject);
      return deferred.promise;
    };

    //
    // shop
    // ----
    //
    // Performs the search that the user
    // has specified in the `Search` object.
    //
    Shopping.shop = function () {
      console.debug('Shopping.shop');

      var search = Search.get();
      var results;

      // perform search
      switch (search.packageType) {
        case "H":
          results = shopHotels(search);
          break;

        case "HT":
          results = shopHotelsAndTickets(search);
          break;

        default:
          throw "Package type not found ..";
      }

      results
        .then(function (res) {

          // THIS SHOULD BE REMOVED AFTER dirtyCustomPackages ARE REMOVED
          if (search.packageType === 'H') {
            var custom = CONFIG.dirtyCustomPackages || [];
            new DirtyCustomPackages(custom)
              .then(function (customPackages) {
                console.debug( 'Shopping.shop', 'applying customPackages' );
                solutions = res.concat(customPackages);
                Shopping.emit('changed');
              });
          }

          else {
            solutions = res;
            Shopping.emit('changed');
          }

        })
        .catch(function (err) {
          if (err.error && err.error.message && err.error.message.errorCode === 'NO_RESULTS_FOUND') {

            // THIS SHOULD BE REMOVED AFTER dirtyCustomPackages ARE REMOVED
            if (search.packageType === 'H') {
              var customPackages = new DirtyCustomPackages(CONFIG.dirtyCustomPackages);
              solutions = customPackages;
            }
            else {
              solutions = [];
            }
            Shopping.emit('changed');
          }
          else {
            throw "Uncaught error from Orbitz: " + JSON.stringify(err);
          }
        });
    };

    //
    // get
    // ---
    //
    // Returns the latest shopping
    // results, if they exist.
    //
    Shopping.get = function () {
      return solutions;
    };

