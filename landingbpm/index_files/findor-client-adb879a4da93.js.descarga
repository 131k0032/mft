.config(function clientConfig(featureFlagsProvider, FLAGS, CONFIG, cfpLoadingBarProvider) {
  console.debug( 'findor-client.config', CONFIG, FLAGS );

  // Enable feature tags from constant.
  if( FLAGS ) {
    featureFlagsProvider.setInitialFlags(FLAGS);
  }

  // Configuration for angular-loading-bar
  cfpLoadingBarProvider.includeSpinner = false;
  cfpLoadingBarProvider.latencyThreshold = 500;
})
.run(function clientRun(gettextCatalog, ENV, CONFIG, featureFlags) {
  console.debug( 'findor-client.run', ENV );

  if( CONFIG.redirection ) {
    var _base = $('base').attr( 'href' );

    // Strip trailing slash from _base since it'll conflict w/ redirection rules.
    if(_base.charAt( _base.length-1 ) === "/") {
      _base = _base.slice(0, -1);
    }

    // Apply redirection rules.
    angular.forEach( CONFIG.redirection, function redirectionRule( target, baseRule ) {

      if( window.location.pathname === ( _base + baseRule ) ) {
        window.location = _base + target;
      }

    });

  }

  // @todo Enable client-config features.
  angular.forEach( ( 'object' === typeof CONFIG && CONFIG.featuresEnabled ? CONFIG.featuresEnabled : [] ), function enableFeature( featureName ) {
    console.debug( 'featureName:enable', featureName );
    featureFlags.enable( featureName );
  });

  // @todo Disable client-config features.
  angular.forEach( ( 'object' === typeof CONFIG && CONFIG.featuresDisabled ? CONFIG.featuresDisabled : [] ), function disableFeature( featureName ) {
    console.debug( 'featureName:disable', featureName );
    featureFlags.disable( featureName );
  });

  // force enable
  featureFlags.enable( 'internalHotels' );

  // Expose certain modules for debug and enable debug mode.
  if (ENV !== 'prod') {
    window.featureFlags = featureFlags;
    gettextCatalog.debug = true;
  }
  
  gettextCatalog.setStrings("es",{
    "Add":"AÃ±adir"
  })

});


