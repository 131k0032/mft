angular
  .module('findor-client')
  .service('hotelHelper', function (orbitzClient, orbitzTracker, CONFIG, Tickets, I18nService, $http, $location, $state, SERVER_BASE_URL) {
    console.debug( 'hotelHelper' );

    var

        /**
         * The currently selected search style
         */
        searchStyle = $location.search().type || 'hotel',

        /**
         * A list of available search styles
         */
        searchStyles = [ 'H', 'HT', 'HTA', 'HA' ],

        /**
         * A representation of the current search.
         *
         * Contains the following properties:
         *
         *  - checkIn:  A moment object representing the check-in date for the search
         *
         *  - checkOut: A moment object representing the check-out date for the search
         *
         *  - rooms:    An ordered collection of the rooms for the search.  Each object
         *              contains details about the room occupants
         */
        shoppingQuery,

        /** 
         * The current shopping search results
         */
        shoppingResults,

        /**
         * An object containing the last specific hotel request that was made
         * 
         * Contains the following properties:
         *
         *  - checkIn:  A moment object representing the check-in date for the search
         *
         *  - checkOut: A moment object representing the check-out date for the search
         *
         *  - rooms:    An ordered collection of the rooms for the search.  Each object
         *              contains details about the room occupants
         *
         *  - hotelId:  A number representing a hotel
         */
        hotelQuery,

        /**
         * The last specific hotel search
         */
        hotelResult,

        /**
         * An object containing the last specific rate rules request that was made
         *
         * Contains the following properties:
         *
         *  - checkIn:  A moment object representing the check-in date for the search
         *
         *  - checkOut: A moment object representing the check-out date for the search
         *
         *  - rooms:    An ordered collection of the rooms for the search.  Each object
         *              contains details about the room occupants
         *
         *  - hotelId:  A number representing a hotel
         *
         *  - roomKeys: An array of room keys
         */
        rateRulesQuery,

        /**
         * The last rate rules request
         */
        rateRulesResponse;

        

    return {
      search: search,
      selectSearchStyle: selectSearchStyle,
      clearTicket: clearTicket,
      setTicket: setTicket,
      getSearchStyle: getSearchStyle,
      getShoppingResults: getShoppingResults,
      searchHotelDetails: searchHotelDetails,
      getHotelDetails: getHotelDetails,
      getRooms: getRooms,
      getTotalPeople: getTotalPeople,
      getRoomKeys: getRoomKeys,
      getLengthOfStay: getLengthOfStay,
      getTicketPrice: getTicketPrice,
      getTicketDiscount: getTicketDiscount,
      getBiggestTicketDiscount: getBiggestTicketDiscount,
      getTicketFees: getTicketFees,
      getTicketTotal: getTicketTotal,
      getHotelTotal: getHotelTotal,
      getPackageTotal: getPackageTotal,
      avgNightlyStrikeRatePerPerson: avgNightlyStrikeRatePerPerson,
      avgNightlyRate: avgNightlyRate,
      avgTotalCostPerPerson: avgTotalCostPerPerson,
      avgTotalStrikeRatePerPerson: avgTotalStrikeRatePerPerson,
      avgNightlyIncrease: avgNightlyIncrease,
      avgTotalIncrease: avgTotalIncrease,
      getRateRulesResponse: getRateRulesResponse,
      rateRulesRequest: rateRulesRequest,
      book: book,
      sameCancellationPolicy: sameCancellationPolicy,
      sendConfirmationEmail: sendConfirmationEmail
    };


    /**
     * Retrieves the length of the currenly requested stay
     */
    function getLengthOfStay () {
      var start, end, range, length;

      if (hotelQuery) {
        start = hotelQuery.checkIn;
        end = hotelQuery.checkOut;
      } else if (shoppingQuery) {
        start = shoppingQuery.checkIn;
        end = shoppingQuery.checkOut;
      }

      range = moment().range(start, end);
      length = 0;

      range.by('days', function () {
        length++;
      });

      return length - 1;
    }



    /**
     * Books the currently loaded rateRules hotel
     */
    function book (queryParams) {
      var rules = rateRulesResponse;

      function appendGuarantee (params) {
        var i, guaranteeType;
        for (i = 0; i < params.rooms.roomRates.length; i++) {
          guaranteeType = _.where(rules.hotel.rooms.roomRates, {room: i+1})[0].roomRate[0].guaranteeType;
          if (guaranteeType) {
            params.rooms.roomRates[i].roomRate = {guaranteeType: guaranteeType};
          }
        }
        return params;
      }

      angular.extend(queryParams, {
        distributionPartnerDetails: {
          path: 'WEBSITE'
        }
      });

      angular.extend(queryParams.rooms, {
        totalCostOfRooms: rules.hotel.rooms.totalCostOfRooms
      });

      queryParams = appendGuarantee(queryParams);
      
      return $http({
        method: 'POST', 
        url: _.where(rules.hotel.rooms.roomRates[0].roomRate[0].links.link, {rel: 'reservation'})[0].href,
        headers: {
          'Content-Type': 'application/json',
          'Accept': 'application/json'
        },
        data: queryParams,
        tracker: orbitzTracker
      }).success(function (data, status, headers, config) {

        queryParams.address = queryParams.paymentCard.address;
        queryParams.phone = queryParams.paymentCard.phone;
        queryParams.paymentCard = undefined;

        if (data.charge && data.charge.error) {
          _LTracker.push({
            event: 'Failed Stripe Transaction',
            source: window.location.origin,
            responseData: data,
            responseHeaders: headers,
            requestData: queryParams,
            status: status
          });
        } else {
          _LTracker.push({
            event: 'Successful Booking',
            source: window.location.origin,
            responseHeaders: headers,
            responseData: data,
            requestData: queryParams,
            status: status
          });
        }

      }).error(function (data, status, headers, config) {

        queryParams.address = queryParams.paymentCard.address;
        queryParams.phone = queryParams.paymentCard.phone;
        queryParams.paymentCard = undefined;
        
        _LTracker.push({
          event: 'Failed Booking',
          source: window.location.origin,
          responseData: data,
          responseHeaders: headers,
          requestData: queryParams,
          status: status
        });
      });
    }



    /**
     * Performs a search for a hotel
     *
     * @param {object} queryParams An object containing the following search parameters:
     *                    - {moment} checkIn The check-in date for the request
     *                    - {moment} checkOut The check-out date for the request
     *                    - {collection} rooms A collection of room details
     *                    - {number} hotelId The hotel we are getting details for
     */
    function searchHotelDetails (queryParams) {

      var rooms = queryParams.rooms;
      hotelQuery = queryParams;

      hotelResult = orbitzClient.shop({
        checkIn: queryParams.checkIn.format('YYYYMMDD'),
        checkOut: queryParams.checkOut.format('YYYYMMDD'),
        hotelId: queryParams.hotelId,
        purchaseCurrency: I18nService.currency().orbitz,
        locale: I18nService.currentLang().orbitz,
        room1: rooms && rooms[0].adults ? buildRoomString(rooms[0]) : undefined,
        room2: rooms && rooms[1].adults ? buildRoomString(rooms[1]) : undefined,
        room3: rooms && rooms[2].adults ? buildRoomString(rooms[2]) : undefined,
        room4: rooms && rooms[3].adults ? buildRoomString(rooms[3]) : undefined,
        contentDetails: 'all',
        multipleRates: true,
        freeCancel: true
      });
    }


    /**
     * Performs a rate rules request
     *
     * @param {object} queryParams An object containing the rate rules requests for the 
     */
    function rateRulesRequest (queryParams, couponCode) {

      var key = queryParams.keys[0];

      if (couponCode) {
        key = key.replace(/NIL/, couponCode);
      }
      
      rateRulesQuery = queryParams;
      rateRulesResponse = orbitzClient.rules({
        key: key,
        queryString: generateRateRulesQueryString(queryParams.keys),
        purchaseCurrency: I18nService.currency().orbitz,
        locale: I18nService.currentLang().orbitz
      });
    }


    /**
     * Retrieves the last rate rules response
     */
    function getRateRulesResponse () {
      return rateRulesResponse;
    }


    /**
     * Returns the latest hotel search results
     *
     * @return If a search has been made, returns the latest search result
     */
    function getHotelDetails () {
      return hotelResult;
    }



    /**
     * Returns the current hotel shopping results
     *
     * @return If a search has been made, returns the current hotel shopping results
     */
    function getShoppingResults () {
      var result;

      if (!shoppingResults)
        result = undefined;

      else if (!shoppingResults.$resolved)
        result = shoppingResults;

      else if (searchStyle === 'HA' || searchStyle === 'HAT') {
        result = [];
        for (var i = 0; i < shoppingResults.packages.length; i += 1) {
          var p = shoppingResults.packages[i];
          var flightId = p.refIds.airRefId;
          var hotelId = p.refIds.hotelRefId;
          var r = {
            flight: shoppingResults.flights[flightId],
            hotel: shoppingResults.hotels[hotelId],
            pricing: p.pricing
          };
          result.push(r);
        }
      }

      else {
        result = shoppingResults;
      }
      return result;
    }



    /**
     * Performs a hotel shopping request
     *
     * @param {moment} checkIn The check-in date for the request
     * @param {moment} checkOut The check-out date for the request
     * @param {collection} rooms A collection of room details
     */
    function search (checkIn, checkOut, rooms) {

      var newQuery = {
        checkIn: checkIn,
        checkOut: checkOut,
        rooms: rooms,
        lang: I18nService.currentLang().orbitz,
        currency: I18nService.currency().orbitz
      };

      if (sameQuery(newQuery, shoppingQuery)) {
        return;
      }

      shoppingQuery = newQuery;

      if (searchStyle === 'HA' || searchStyle === 'HAT') {
        shoppingResults = orbitzClient.shopPackages({
          OriginCode: $location.search()['fromAirport'],
          DestinationCode: $location.search()['toAirport'],
          StartDate: checkIn.format('YYYY-MM-DD'),
          EndDate: checkOut.format('YYYY-MM-DD'),
          NumberOfRooms: '1',
          AdultCount: rooms[0].adults,
          AnchorType: 'CheapestAir',
          PreferNonStop: 'True'
        });
      } 

      else {
        shoppingResults = orbitzClient.shop({
          checkIn: checkIn.format('YYYYMMDD'),
          checkOut: checkOut.format('YYYYMMDD'),
          locKeyword: CONFIG.search.city,
          purchaseCurrency: newQuery.currency,
          locale: newQuery.lang,
          room1: rooms[0].adults ? buildRoomString(rooms[0]) : undefined,
          room2: rooms[1].adults ? buildRoomString(rooms[1]) : undefined,
          room3: rooms[2].adults ? buildRoomString(rooms[2]) : undefined,
          room4: rooms[3].adults ? buildRoomString(rooms[3]) : undefined,
          contentDetails: 'all',
          rateDetails: 'medium',
          freeCancel: true,
          totalStrikeRate: true
        });
      }
    }

    /**
     * Generates a standard rooms object
     *
     * @param {ui.router stateParams} stateParams The stateParams for the view
     * @return {collection} A collection, where each object contains the occupants of the room
     */
    function getRooms (stateParams) {
      return [
          { adults: stateParams.room1 }, 
          { adults: stateParams.room2 }, 
          { adults: stateParams.room3 }, 
          { adults: stateParams.room4 }
        ];
    }

    /**
     * Generates a room keys object
     *
     * @param {ui.router stateParams} stateParams The stateParams for the view
     * @return {collection} A collection of room keys
     */
    function getRoomKeys (stateParams) {
      var keys = [];
      _.forEach(['room1Key', 'room2Key', 'room3Key', 'room4Key'], function (key) {
        if (stateParams[key]) {
          keys.push(stateParams[key]);
        }
      });
      return keys;
    }

    /**
     * Generates the "addroom" query string for a rate rules request
     *
     * @param {array} keys An array of room keys
     */
    function generateRateRulesQueryString (keys) {
      var qs = "";
      if (keys.length > 1) {
        qs = "?addRoom=" + keys[1];
        qs += keys[2] ? "&addRoom=" + keys[2] : '';
        qs += keys[3] ? "&addRoom=" + keys[3] : '';
      } 
      return qs;
    }

    /**
     * A helper function used to build a room query string
     * 
     * @param {object} room The occupancy details for the given room
     * @return {string} The parameter for the query string
     */
    function buildRoomString (room) {
      return room.adults + ',0';
    }

    /**
     * Checks whether two shopping queries are the same
     */
    function sameQuery (first, second) {
      if ( ! first || ! second) {
        return false;
      }

      if ( ! _.isEqual(first.rooms, second.rooms)) {
        return false;
      }

      if (first.lang !== second.lang || first.currency !== second.currency) {
        return false;
      }

      if (first.checkIn.diff(second.checkIn, 'days') !== 0) {
        return false;
      }

      if (first.checkOut.diff(second.checkOut, 'days') !== 0) {
        return false;
      }

      return true;
    }

    /**
     * Selects the shopping style.
     *
     * @param {string} style The style to be selected.  Can be "hotel" or "package".
     */
    function selectSearchStyle (style) {
      if ( ! _.contains(searchStyles, style)) {
        return false;
      }
      searchStyle = style;
      $state.params.type = style;
    }

    function clearTicket () {
      selectSearchStyle('H');
    }

    function setTicket (id) {
      selectSearchStyle('HT');
      Tickets.set(id);
    }

    /**
     * Returns the current shopping style.
     * 
     * @returns {string} The current shopping style
     */
    function getSearchStyle () {
      return $location.search().type || 'hotel';
    }    

    /**
     * Returns the average total price including tickets
     */
    function avgTotalCostPerPerson (totalCost, totalPeople) {
      var hotelCostPerPerson = totalCost / totalPeople;
      return hotelCostPerPerson + getTicketPrice();
    }

    /**
     * Returns the average total strike rate per person including tickets
     */
    function avgTotalStrikeRatePerPerson (totalCost, totalStrikeRate, totalPeople) {
      var strikeRatePerPerson = totalStrikeRate === 0 ? totalCost / totalPeople : totalStrikeRate / totalPeople;
      return strikeRatePerPerson + getTicketPrice() + getTicketDiscount();
    }

    /**
     * Calculates the average nightly strike rate per person
     */
    function avgNightlyStrikeRatePerPerson (totalStrikeRate, lengthOfStay) {
      return totalStrikeRate / lengthOfStay;
    }

    /**
     * Calculates the average nightly rate per person
     */
    function avgNightlyRate (total, lengthOfStay) {
      return total / lengthOfStay;
    }

    /**
     * Calculates avg total price increase from the cheapest room for a given room
     */
    function avgTotalIncrease (roomTotal, cheapest, totalPeople, lengthOfStay) {
      return avgTotalCostPerPerson(roomTotal, totalPeople) - avgTotalCostPerPerson(cheapest, totalPeople);
    }

    /**
     * Calculates the average nightly price increase from the cheapest room for a given room
     */
    function avgNightlyIncrease (roomTotal, cheapest, lengthOfStay) {
      return avgNightlyRate(roomTotal, lengthOfStay) - avgNightlyRate(cheapest, lengthOfStay);
    }

    /**
     * Returns the total number of people in all supplied rooms
     */
    function getTotalPeople (rooms) {
      return _.reduce(rooms, function (sum, room) {
        return room.adults ? sum + parseInt(room.adults) : sum;
      }, 0);
    }

    /**
     * Returns the price of a single ticket
     */
    function getTicketPrice () {
      if (Tickets.empty) return 0;
      if (Tickets.current === null) return 0;
      return Tickets.current.price.value * I18nService.getConversion().rate;
    }

    /**
     * Returns the taxes and fees for a single ticket
     */
    function getTaxesAndFees () {
      if (Tickets.empty) return 0;
      if (Tickets.current === null) return 0;
      return (Tickets.current.taxesAndFees.value || 0) * I18nService.getConversion().rate;
    }

    /**
     * Returns the discount applied by buying the tickets through findor
     */
    function getTicketDiscount () {
      if (Tickets.empty) return 0;
      if (Tickets.current === null) return 0;
      return Tickets.current.discount.value * I18nService.getConversion().rate;
    }

    /**
     * Returns the biggest discount you can get with any ticket
     */
    function getBiggestTicketDiscount () {
      if (Tickets.empty) return 0;
      if (Tickets.current === null) return 0;
      var biggestDiscount = 0;
      _.each(Tickets.list, function(ticket) {
        if (ticket.discount > biggestDiscount) {
          biggestDiscount = ticket.discount;
        }
      });
      return biggestDiscount * I18nService.getConversion().rate;
    }


    /**
     * Returns the total applicable fees
     */
    function getTicketFees (totalPeople) {
      if (Tickets.empty) return 0;
      if (Tickets.current === null) return 0;
      var currency = I18nService.currency().display
        , taxesAndFees = getTaxesAndFees() * totalPeople
        , shippingFee = (Tickets.current.shippingFee.value || 0) * I18nService.getConversion().rate
      ;
      return taxesAndFees + shippingFee;
    }

    /**
     * Returns the total price of the tickets
     */
    function getTicketTotal (totalPeople) {
      if (Tickets.empty) return 0;
      if (Tickets.current === null) return 0;
      return totalPeople * getTicketPrice() + getTicketFees(totalPeople);
    }

    /**
     * Returns the total price of the hotel
     */
    function getHotelTotal () {
      return rateRulesResponse.hotel ? rateRulesResponse.hotel.totalCostInclusive : 0;
    }

    /**
     * Returns the total cost of tickets + admission
     */
    function getPackageTotal(totalPeople) {
      if (Tickets.empty) return null;
      if (Tickets.current === null) return 0;
      return getTicketTotal(totalPeople) + getHotelTotal();
    }

    /**
     * Returns true if the cancellation policies for all the rooms are the same
     */
    function sameCancellationPolicy () {
      if (!rateRulesResponse.hotel) return false;
      var reduce = _.reduce(rateRulesResponse.hotel.rooms.roomRates, function (same, roomRates) {
        if (same === true) {
          return roomRates.roomRate[0].cancelPolicy.description.value;
        } else if (same === false) {
          return false;
        } else {
          return same === roomRates.roomRate[0].cancelPolicy.description.value;
        }
      }, true);

      return _.isString(reduce);
    }

    /**
     * Sends the confirmation email
     * @param {string} confirmationCode The confirmation code for the booking
     * @param {string} html The html content for the email
     */
    function sendConfirmationEmail (confirmationCode, html) {

      var email, emailParams;

      email = angular.extend(CONFIG.confirmationEmail, {
        html: html,
        key: confirmationCode
      });

      emailParams = {
        method: 'POST',
        url: SERVER_BASE_URL + '/email/' + confirmationCode,
        data: email,
        tracker: orbitzTracker
      };

      return $http(emailParams);
    }
  });angular
