  .module('findor-client')
  .filter('distance', function (Currency) {
    return function (value) {

      var distance, unit
        , kmToMileRatio = 0.621371
        , mileToFootRatio = 5280
        , mileToFootCutoff = 0.189394 // 1000 feet in miles
        , kmUnit = 'km'
        , mUnit = 'm'
        , mileUnit = 'mi'
        , feetUnit = 'ft'
        , imperial = (_.indexOf(['USD', 'GBP'], Currency.get().display) > -1);

      // display in imperial
      if (imperial) {
        value = value * kmToMileRatio;
        if (value >= 100) {
          distance = Math.round(value);
          unit = 'mi';
        } else if (value >= 10) {
          distance = Math.round(value * 10) / 10;
          unit = 'mi';
        } else if (value >= 1) {
          distance = Math.round(value * 100) / 100;
          unit = 'mi';
        } else if (value >= mileToFootCutoff) {
          distance = Math.round(value * 1000) / 1000;
          unit = 'mi';
        } else {
          distance = Math.round(value * mileToFootRatio);
          unit = 'ft';
        }
      }

      // display in metric
      else {
        if (value >= 100) {
          distance = Math.round(value);
          unit = 'km';
        } else if (value >= 1) {
          distance = Math.round(value * 10) / 10;
          unit = 'km';
        } else {
          distance = Math.round(value * 1000);
          unit = 'm';
        }
      }

      return distance + ' ' + unit;
    };
  });angular
