          } else if (roomRate.roomRate[0].supplierFees
                     && roomRate.roomRate[0].supplierFees.bookingFees
                     && roomRate.roomRate[0].supplierFees.bookingFees.fee) {
            return sum.concat(roomRate.roomRate[0].supplierFees.bookingFees.fee);
          }
        }, []);
      };

      data.getLocalCharges = function () {
        return _.reduce(data.rooms.roomRates, function (sum, roomRate) {
          if (roomRate.roomRate[0].hotelFees) {
            return sum.concat(_.where(roomRate.roomRate[0].hotelFees.hotelFee, {collectedAtBooking: false}));
          } else if (roomRate.roomRate[0].supplierFees 
                     && roomRate.roomRate[0].supplierFees.checkoutFees 
                     && roomRate.roomRate[0].supplierFees.checkoutFees.fee) {
            return sum.concat(roomRate.roomRate[0].supplierFees.checkoutFees.fee);
          }
        }, []);
      };

      data.getLocalChargesTotal = function () {
        var rules, sum;
        rules = this;
        sum = _.reduce(data.rooms.roomRates, function (sum, roomRate) {
          if (roomRate.roomRate[0].supplierFees) {
