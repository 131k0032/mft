    return function (packages) {
      var code = Currency.get().display;
      var price;
      var promises = [];
      var deferred = $q.defer();

      if (!packages)
        return [];

      var setSubtotal = function (newPrice) {
        this.subTotal = newPrice.value;
      };

      var setTotal = function (newPrice) {
        this.total = newPrice.value;
      };

      var i;
      var promise;
      for (i = 0; i < packages.length; i += 1) {
        var p = packages[i];
        if (p.orbitz.subTotal) {
          promise = new Price({
              value: p.orbitz.subTotal.amount,
              currency: p.orbitz.subTotal.currency
            })
            .convertTo(code);

          promise.then(setSubtotal.bind(p));
          promises.push(promise);
        }
        if (p.orbitz.total) {
          promise = new Price({
              value: p.orbitz.total.amount,
              currency: p.orbitz.total.currency
            })
            .convertTo(code);

          promise.then(setTotal.bind(p));
          promises.push(promise);
        }
      }

      $q.all(promises)
        .then(function () {
          deferred.resolve(packages);
        })
        .catch(deferred.reject);

      return deferred.promise;
    };
