// Geometry
// --------
//
// The geometry service is responsible
// for doing geometric calculations
// within the application.  Generally,
// these are related to distances or
// drawing on the map.
//
angular
  .module('findor-client')
  .service('Geometry', function () {
    
    var Geometry = {};
    var approximateEarthRadius = 6371;

    function rad (val) {
      return val * Math.PI / 180;
    }

    function deg (val) {
      return val * 180 / Math.PI;
    }

    //
    // distance
    // --------
    //
    // This method calculates the distance
    // between two points, `first` and `second`.
    // Both `first` and `second` should have
    // the properties `lat` and `lng`,
    // representing the latitude and longitude
    // of the two points
    //
    // The value returned is in kilometers.
    //
    Geometry.distance = function (first, second) {
      var x1 = rad(first.lat),
          x2 = rad(second.lat),
          y = rad(second.lng - first.lng);

      return Math.acos(Math.sin(x1) * Math.sin(x2) + Math.cos(x1) * Math.cos(x2) * Math.cos(y)) * approximateEarthRadius;
    };

    // 
    // getRotationToVertical
    // ---------------------
    //
    // This method is specifically used for rotating
    // the Findor map to ensure that the line between
    // the hotel and the venue is vertical.
    //
    // The parameters sent, `bottom` and `top`, should
    // both be points in geographical space, and should
    // both contain `lat` and `lng` properties.
    //
    // The value returned is the rotation (in degrees)
    // that needs to be applied to the map.
    //
    Geometry.getRotationToVertical = function (top, bottom) {
      var midPoint = L.latLng(top.lat, bottom.lng),
          topPoint = L.latLng(top.lat, top.lng),
          botPoint = L.latLng(bottom.lat, bottom.lng),
          dy = botPoint.distanceTo(midPoint),
          dx = topPoint.distanceTo(midPoint),
          y = top.lat - bottom.lat,
          x = top.lng - bottom.lng,
          angle = deg(Math.atan(dx / dy)),
          neg = ((y >= 0 && x >= 0) || (y < 0 && x < 0)),
          result = neg ? - angle : angle;

      return result;
    };

    Geometry.getRotationToHorizontal = function (left, right) {
      return Geometry.getRotationToVertical(left, right) - 90;
    };

    return Geometry;
  });//
