angular
  .module('findor-client')
  .service('CurrencyConversionAccess', function ($http, $q, $cacheFactory, CurrencyConversionTracker) {

    var CurrencyConversionAccess = {};
    var cache = $cacheFactory('CurrencyConversionAccess');
    var CURRENCY_CONVERSION_ENDPOINT = 'https://openexchangerates.org/api/latest.json?app_id=d11a81daa08d491b9e0805b08430a7aa';
    var CURRENCY_CONVERSION_EXPIRY = 3600000;

    // 
    // get
    // ---
    //
    // Retrieves conversion rates from the 
    // provided `base` currency to other
    // currencies.
    // 
    CurrencyConversionAccess.get = function (base) {
      var deferred = $q.defer();
      var cached = cache.get(base);

      function pollCache () {
        cached = cache.get(base);
        if (cached === 'waiting')
          window.setTimeout(pollCache, 500);
        else 
          deferred.resolve(cached.rates);
      }

      if (cached)
        pollCache();

      else {
        var request = $http({
          method: 'JSONP',
          url: CURRENCY_CONVERSION_ENDPOINT,
          params: {
            base: base,
            callback: 'JSON_CALLBACK'
          },
          tracker: CurrencyConversionTracker
        });

        request
          .success(function (data, status, headers, config) {
            cache.put(base, data);
            deferred.resolve(data.rates);
          })
          .error(function (data, status, headers, config) {
            deferred.reject(data);
          });

        cache.put(base, 'waiting');
      }

      return deferred.promise;
    };

