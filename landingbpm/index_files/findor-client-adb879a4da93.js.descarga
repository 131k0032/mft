// Repricer
// --------
//
// The `Repricer` is responsible for repricing
// a hotel room (or set of rooms).
//
// The flow for this module is a bit weird.
// In order to make a reprice request, the assumption
// is that a Customization request has already
// been made.  
//
angular
  .module('findor-client')
  .service('Repricer', function ($q, HotelAccess, Currency, Language, Customization, Search, Hotel) {
    var Repricer = new EventEmitter();
    var reprice;
    var locked = false;
    var repricePromise;
    var lastHotelId;

    //
    // buildPackage
    // ------------
    //
    // This method gets called after a customization
    // shopping request has been made.  It then
    // waits for a reprice request to complete, and
    // updates the details.
    // 
    var buildPackage = function () {
      var pac = Customization.get();
      var keys;
      var hotel;
      Customization.removeListener('changed', buildPackage);
      repricePromise
        .then(function (hotelReprice) {
          keys = Search.get().getKeys();
          hotel = new Hotel(hotelReprice, keys);

          for (var key in pac.orbitz) {
            if (!hotel[key]) hotel[key] = pac.orbitz[key];
          }

          pac.setOrbitzItem(hotel);
          reprice = pac;

          Repricer.emit('changed');
        })
        .catch(function (err) {
          reprice = err;
          Repricer.emit('changed');
        });
    };

    //
    // reprice
    // -------
    //
    // Performs a reprice for the provided
    // hotel.
    //
    Repricer.reprice = function (hotelId) {
      console.debug( 'Repricer.reprice', hotelId );

      var keys = Search.get().getKeys();

      if (!hotelId) hotelId = lastHotelId;
      else lastHotelId = hotelId;

      repricePromise = HotelAccess.reprice({
        // Should have a better/safer way of getting checkIn/out dates than this. - potanin@UD
        checkIn: Search.get().checkIn ? Search.get().checkIn.format( 'YYYYMMDD' ) : null,
        checkOut: Search.get().checkOut ? Search.get().checkOut.format( 'YYYYMMDD' ) : null,
        keys: keys,
          locale: Language.get().orbitz,
          purchaseCurrency: Currency.get().orbitz
        }, hotelId);
      
      Customization.build(hotelId);
      Customization.addListener('changed', buildPackage);
    };

    //
    // get
    // ---
    // 
    // Returns the current reprice.
    //
    Repricer.get = function () {
      return reprice;
    };

    return Repricer;
