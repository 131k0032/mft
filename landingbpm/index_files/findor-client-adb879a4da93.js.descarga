    var geolocationCoordinates;
    var geocodeResponse;

    $scope.geocodeAndGoToFlight = function () {
      Loader.setLoading();
      if (!gotCookies) {
        geolocationDeclined = $cookieStore.get("geolocationDeclined");
        geolocationCoordinates = $cookieStore.get("geolocationCoordinates");
        geocodeResponse = $cookieStore.get("geocodeResponse");
        gotCookies = true;
      }
      if (geolocationDeclined === true) {
        openFlightURL("");
      }
      else if (geocodeResponse !== undefined) {
        geocodeSuccess(geocodeResponse);
      }
      else if (geolocationCoordinates !== undefined) {
        geolocationSuccess(geolocationCoordinates);
      }
      else if (geolocationDeclined !== true && $window.navigator.geolocation) {
        $window.navigator.geolocation.getCurrentPosition(function (position) {
          geolocationSuccess({ lat: position.coords.latitude, lng: position.coords.longitude });
        }, geolocationError);
      }
    };
    $scope.showChat = function () {
      $zopim(function () {
        $zopim.livechat.window.toggle();
      });
    }
    function getFlightURL(origCity) {
      origCity = encodeURIComponent(origCity);
      var destCity = encodeURIComponent(CONFIG.city);
      var leaveDate = encodeURIComponent($scope.search.checkIn.format('MM/DD/YY'));
      var returnDate = encodeURIComponent($scope.search.checkOut.format('MM/DD/YY'));
      var numAdult = $scope.search.numGuests;
      return "http://orbitz.com/shop/home?type=air&ar.type=roundTrip&strm=true&ar.rt.leaveSlice.orig.key=" + origCity + "&_ar.rt.leaveSlice.originRadius=0&ar.rt.leaveSlice.originRadius=80.0%20mi&ar.rt.leaveSlice.dest.key=" + destCity + "&_ar.rt.leaveSlice.destinationRadius=0&ar.rt.leaveSlice.destinationRadius=80.0%20mi&ar.rt.leaveSlice.date=" + leaveDate + "&ar.rt.leaveSlice.time=Anytime&ar.rt.returnSlice.date=" + returnDate + "&ar.rt.returnSlice.time=Anytime&_ar.rt.flexAirSearch=0&ar.rt.numAdult=" + numAdult + "&ar.rt.numSenior=0&ar.rt.numChild=0&ar.rt.child%5B0%5D=&ar.rt.child%5B1%5D=&ar.rt.child%5B2%5D=&ar.rt.child%5B3%5D=&ar.rt.child%5B4%5D=&ar.rt.child%5B5%5D=&ar.rt.child%5B6%5D=&ar.rt.child%5B7%5D=&search=Search%20Flights&_ar.rt.nonStop=0&_ar.rt.narrowSel=0&ar.rt.narrow=airlines&ar.rt.carriers%5B0%5D=&ar.rt.carriers%5B1%5D=&ar.rt.carriers%5B2%5D=&ar.rt.cabin=C&view.sortType=AIR_PRICE&view.sortAscending=true&statFlexSearch=false";
    }

    function openFlightURL(origCity) {
      $window.location = getFlightURL(origCity);
    }


    /* jshint ignore:start */
    $scope.googleLatLng = function (lat, lng) {
      if (google) {
        return new google.maps.LatLng(lat, lng);
      }

      return null;
    }

    $scope.googleGeocode = function (latlng, successFunction, errorFunction) {

      if (!google || !google.maps) {
        return errorFunction();
      }

      var geocoder = new google.maps.Geocoder();
      geocoder.geocode({ 'latLng': latlng }, function (results, status) {
        if (status == google.maps.GeocoderStatus.OK) {
          successFunction(results);
        }
        else {
          errorFunction(status);
        }
      });
    }
    /* jshint ignore:end */

    function geolocationSuccess(coordinates) {
      geolocationDeclined = false;
      geolocationCoordinates = coordinates;
      $cookieStore.put("geolocationDeclined", false);
      $cookieStore.put("geolocationCoordinates", coordinates);
      geocode(coordinates.lat, coordinates.lng, geocodeSuccess, geocodeError);
    }

    function geolocationError() {
      geolocationDeclined = true;
      $cookieStore.put("geolocationDeclined", true);
    }

    function geocodeSuccess(response) {
      geocodeResponse = response;
      $cookieStore.put("geocodeResponse", response);
      $timeout(function () {
        openFlightURL(response.city.long_name + ", " + response.region.short_name);
      }, 1);
    }

    function geocodeError() {
      //console.log("Geocode failed.");
    }

    function geocode(lat, lng, successFunction, errorFunction) {
      $scope.googleGeocode($scope.googleLatLng(lat, lng), function (results) {
        if (results[1]) {
          var indice = 0;
          for (var j = 0; j < results.length; j++) {
            if (results[j].types[0] === 'locality') {
              indice = j;
              break;
            }
          }
          var city = {};
          var region = {};
          var country = {};
          for (var i = 0; i < results[j].address_components.length; i++) {
            if (results[j].address_components[i].types[0] === "locality") {
              city = results[j].address_components[i];
            }
            if (results[j].address_components[i].types[0] === "administrative_area_level_1") {
              region = results[j].address_components[i];
            }
            if (results[j].address_components[i].types[0] === "country") {
              country = results[j].address_components[i];
            }
          }
          successFunction({
            city: city,
            region: region,
            country: country
          });
        }
        else {
          errorFunction();
        }
      }, function (status) {
        console.log("Geocode failed due to: " + status);
      });

    }
    






