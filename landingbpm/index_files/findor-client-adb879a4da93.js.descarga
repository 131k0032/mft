// ------------------------
// usage: {{priceValue | convertedCurrency: fromCurrency}}
//
// convert price value from the specified currency to the app's global currency
// use CurrencyConverter service to asynchronously convert the price
//
// ** modified to round the value not to have decimals
// ** modified to make rounding ability optional
//
angular
  .module('findor-client')
  .filter('convertedCurrency', function ($filter, $cacheFactory, Currency, CurrencyConverter) {

    var allCurrencies = Currency.getAll();
    var resultsCache = $cacheFactory('convertedCurrency.filter');

    return filterFn;
    ////////////////
    function filterFn(value, fromCurrency, showDecimal) {
      var targetValue = null;
      var serviceInvoked = false;
      var toCurrency = Currency.get().orbitz;
      var toSymbol = allCurrencies[toCurrency].symbol;

      if (fromCurrency === toCurrency) {
        return (toSymbol == "â¬" ? '' : toSymbol) + '' +formatCurrency(round10(value, showDecimal),toSymbol);
      }

      var key = JSON.stringify([
        value,
        fromCurrency,
        toCurrency
      ]);
      var cached = resultsCache.get(key);

      if (cached) {
        return cached;
      }

      if (targetValue === null) {
        if (!serviceInvoked) {
          serviceInvoked = true;
          CurrencyConverter
            .convert(value, fromCurrency, toCurrency)
            .then(function (convertedValue) {
              targetValue = (toSymbol == "â¬" ? '' : toSymbol) + '' +formatCurrency(round10(convertedValue, showDecimal),toSymbol);
              resultsCache.put(key, targetValue);
            });
        }
        return '-';
      } else {
        return targetValue;
      }
    }

    function round10(value, showDecimal) {
      if (!showDecimal) {
        return Math.round(value);
      } else {
        return Math.round((value + 0.00001) * 100) / 100;
      }
    }
    function formatCurrency(num, currency) {
      if (currency == 'â¬') {
        var millard = '.';
        var decimal = ',';
      } else {
        var millard = ',';
        var decimal = '.';
      }
      // NO GLOBAL VAR SHOULD BE EXPOSED!
      var L;
      var parts = ('' + (num < 0 ? -num : num)).split("."), s = parts[0], i = L = s.length, o = '', c;
      while (i--) {
        o = (i == 0 ? '' : ((L - i) % 3 ? '' : millard))
        + s.charAt(i) + o
      }
      return (num < 0 ? '-' : '') + o + (parts[1] ? decimal + parts[1] : '');


    }
  });
